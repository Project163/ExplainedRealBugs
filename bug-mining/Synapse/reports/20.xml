      &amp;lt;soapenv:Header&amp;gt;&lt;br/&gt;
         &amp;lt;wsa:MessageID&amp;gt;urn:uuid:3429C71AB558E2C4FB1178173070225&amp;lt;/wsa:MessageID&amp;gt;&lt;br/&gt;
         &amp;lt;wsa:Action&amp;gt;urn:getQuote&amp;lt;/wsa:Action&amp;gt;&lt;br/&gt;
      &amp;lt;/soapenv:Header&amp;gt;&lt;br/&gt;
      &amp;lt;soapenv:Body&amp;gt;&lt;br/&gt;
         &amp;lt;soapenv:Fault&amp;gt;&lt;br/&gt;
            &amp;lt;faultcode xmlns:tns=&quot;http://www.w3.org/2003/05/soap-envelope&quot;&amp;gt;tns:Receiver&amp;lt;/faultcode&amp;gt;&lt;br/&gt;
            &amp;lt;faultstring&amp;gt;&amp;lt;soapenv:Fault xmlns:soapenv=&quot;http://schemas.xmlsoap.org/soap/envelope/&quot;&amp;gt;&amp;lt;faultcode&amp;gt;soapenv:Client&amp;lt;/faultcode&amp;gt;&amp;lt;faultstring&amp;gt;java.net.UnknownHostException: bogus&amp;lt;/faultstring&amp;gt;&amp;lt;detail /&amp;gt;&amp;lt;/soapenv:Fault&amp;gt;&lt;br/&gt;
         &amp;lt;/faultstring&amp;gt;&lt;br/&gt;
         &amp;lt;detail /&amp;gt;&lt;br/&gt;
      &amp;lt;/soapenv:Fault&amp;gt;&lt;br/&gt;
   &amp;lt;/soapenv:Body&amp;gt;&lt;br/&gt;
&amp;lt;/soapenv:Envelope&amp;gt;&lt;br/&gt;
**************************************************************************************************************************************************************************************&lt;/p&gt;</description>
