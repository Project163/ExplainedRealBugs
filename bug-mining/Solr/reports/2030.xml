        &lt;span class=&quot;code-quote&quot;&gt;&quot;endOffset&quot;&lt;/span&gt;:14,
        &lt;span class=&quot;code-quote&quot;&gt;&quot;suggestion&quot;&lt;/span&gt;:[&lt;span class=&quot;code-quote&quot;&gt;&quot;solra&quot;&lt;/span&gt;,
