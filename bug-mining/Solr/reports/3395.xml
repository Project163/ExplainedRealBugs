&lt;pre&gt;      &quot;updateHandler&quot;:{
        &quot;class&quot;:&quot;org.apache.solr.update.DirectUpdateHandler2&quot;,
        &quot;description&quot;:&quot;Update handler that efficiently directly updates the on-disk main lucene index&quot;,
        &quot;stats&quot;:null},
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;</description>
