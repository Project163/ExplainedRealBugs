[ &lt;a href=&quot;https://gitbox.apache.org/repos/asf?p=solr.git;h=c9d656a8aa7&quot; class=&quot;external-link&quot; target=&quot;_blank&quot; rel=&quot;nofollow noopener&quot;&gt;https://gitbox.apache.org/repos/asf?p=solr.git;h=c9d656a8aa7&lt;/a&gt; ]&lt;/p&gt;

