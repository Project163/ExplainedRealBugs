        at org.apache.solr.handler.admin.CollectionsHandler.handleResponse(CollectionsHandler.java:364)&lt;br/&gt;
        at org.apache.solr.handler.admin.CollectionsHandler.handleResponse(CollectionsHandler.java:320)&lt;br/&gt;
        at org.apache.solr.handler.admin.CollectionsHandler.handleRemoveReplica(CollectionsHandler.java:494)&lt;br/&gt;
