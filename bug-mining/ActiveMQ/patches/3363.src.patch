+        msg.setStringProperty("color", "blue");
+        producer.send(msg);
+        msg.setText("Msg:2");
+        msg.setStringProperty("color", "red");
+        producer.send(msg);
+
+        assertTextMessageEquals("Msg:2", consumer.receive(5000));
+
+        assertNull(consumerNoMatch.receiveNoWait());
+
