+        printResults("MSGS", result);
+        statement.close();
+
+        statement = conn.prepareStatement("SELECT * FROM ACTIVEMQ_ACKS");
+        result = statement.executeQuery();
+        printResults("ACKS", result);
+        statement.close();
+
