        at javax.servlet.http.HttpServlet.service(HttpServlet.java:693)&lt;br/&gt;
        at org.eclipse.jetty.websocket.WebSocketServlet.service(WebSocketServlet.java:73)&lt;br/&gt;
        at javax.servlet.http.HttpServlet.service(HttpServlet.java:806)&lt;br/&gt;
        at org.eclipse.jetty.servlet.ServletHolder.handle(ServletHolder.java:527)&lt;br/&gt;
