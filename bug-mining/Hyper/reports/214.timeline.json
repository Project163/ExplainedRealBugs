s/sparky8251/starred{/owner}{/repo}","subscriptions_url":"https://api.github.com/users/sparky8251/subscriptions","organizations_url":"https://api.github.com/users/sparky8251/orgs","repos_url":"https://api.github.com/users/sparky8251/repos","events_url":"https://api.github.com/users/sparky8251/events{/privacy}","received_events_url":"https://api.github.com/users/sparky8251/received_events","type":"User","user_view_type":"public","site_admin":false}},{"url":"https://api.github.com/repos/hyperium/hyper/issues/comments/543181958","html_url":"https://github.com/hyperium/hyper/issues/1944#issuecomment-543181958","issue_url":"https://api.github.com/repos/hyperium/hyper/issues/1944","id":543181958,"node_id":"MDEyOklzc3VlQ29tbWVudDU0MzE4MTk1OA==","user":{"login":"zimond","id":4921289,"node_id":"MDQ6VXNlcjQ5MjEyODk=","avatar_url":"https://avatars.githubusercontent.com/u/4921289?v=4","gravatar_id":"","url":"https://api.github.com/users/zimond","html_url":"https://github.com/zimond","followers_url":"https://api.github.com/users/zimond/followers","following_url":"https://api.github.com/users/zimond/following{/other_user}","gists_url":"https://api.github.com/users/zimond/gists{/gist_id}","starred_url":"https://api.github.com/users/zimond/starred{/owner}{/repo}","subscriptions_url":"https://api.github.com/users/zimond/subscriptions","organizations_url":"https://api.github.com/users/zimond/orgs","repos_url":"https://api.github.com/users/zimond/repos","events_url":"https://api.github.com/users/zimond/events{/privacy}","received_events_url":"https://api.github.com/users/zimond/received_events","type":"User","user_view_type":"public","site_admin":false},"created_at":"2019-10-17T13:45:35Z","updated_at":"2019-10-17T13:46:34Z","body":"@sparky8251 Currently you need to create a new executor, something like this (not tested):\r\n\r\n```rust\r\n\r\nstruct DefaultExecutor(TaskExecutor);\r\n\r\n\r\nimpl Executor for DefaultExecutor {\r\n    fn spawn(\r\n        &mut self,\r\n        future: Pin<Box<dyn Future<Output = ()> + Send>>\r\n    ) -> Result<(), tokio_executor::SpawnError> {\r\n        Ok(self.0.spawn(future))\r\n    }\r\n\r\n    fn status(&self) -> Result<(), tokio_executor::SpawnError> {\r\n        self.0.status()\r\n    }\r\n}\r\n\r\nimpl Executor for &DefaultExecutor {\r\n    fn spawn(\r\n        &mut self,\r\n        future: Pin<Box<dyn Future<Output = ()> + Send>>\r\n    ) -> Result<(), tokio_executor::SpawnError> {\r\n        Ok(self.0.spawn(future))\r\n    }\r\n\r\n    fn status(&self) -> Result<(), tokio_executor::SpawnError> {\r\n        self.0.status()\r\n    }\r\n}\r\n```","author_association":"NONE","reactions":{"url":"https://api.github.com/repos/hyperium/hyper/issues/comments/543181958/reactions","total_count":0,"+1":0,"-1":0,"laugh":0,"hooray":0,"confused":0,"heart":0,"rocket":0,"eyes":0},"performed_via_github_app":null,"event":"commented","actor":{"login":"zimond","id":4921289,"node_id":"MDQ6VXNlcjQ5MjEyODk=","avatar_url":"https://avatars.githubusercontent.com/u/4921289?v=4","gravatar_id":"","url":"https://api.github.com/users/zimond","html_url":"https://github.com/zimond","followers_url":"https://api.github.com/users/zimond/followers","following_url":"https://api.github.com/users/zimond/following{/other_user}","gists_url":"https://api.github.com/users/zimond/gists{/gist_id}","starred_url":"https://api.github.com/users/zimond/starred{/owner}{/repo}","subscriptions_url":"https://api.github.com/users/zimond/subscriptions","organizations_url":"https://api.github.com/users/zimond/orgs","repos_url":"https://api.github.com/users/zimond/repos","events_url":"https://api.github.com/users/zimond/events{/privacy}","received_events_url":"https://api.github.com/users/zimond/received_events","type":"User","user_view_type":"public","site_admin":false}},{"id":2721352748,"node_id":"MDE0Ok1lbnRpb25lZEV2ZW50MjcyMTM1Mjc0OA==","url":"https://api.github.com/repos/hyperium/hyper/issues/events/2721352748","actor":{"login":"sparky8251","id":1992688,"nod