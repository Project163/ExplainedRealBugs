\n    /// #         id -> Integer,\r\n    /// #         range -> Range<Integer>,\r\n    /// #     }\r\n    /// # }\r\n    /// #\r\n    /// # fn main() {\r\n    /// #     run_test().unwrap();\r\n    /// # }\r\n    /// #\r\n    /// # fn run_test() -> QueryResult<()> {\r\n    /// #     use self::posts::dsl::*;\r\n    /// #     let conn = establish_connection();\r\n    /// #     conn.execute(\"DROP TABLE IF EXISTS posts\").unwrap();\r\n    /// #     conn.execute(\"CREATE TABLE posts (id SERIAL PRIMARY KEY, range INT4RANGE NOT NULL)\").unwrap();\r\n    /// #\r\n    /// diesel::insert_into(posts)\r\n    ///     .values(&vec![\r\n    ///         range.eq((Bound::Included(3), Bound::Included(5))),\r\n    ///         range.eq((Bound::Included(3), Bound::Excluded(5))),\r\n    ///         range.eq((Bound::Excluded(3), Bound::Unbounded)),\r\n    ///     ])\r\n    ///     .execute(&conn)?;\r\n    ///\r\n    /// let data = posts.select(id)\r\n    ///     .filter(range.overlaps_with((Bound::Included(5), Bound::Excluded(7))))\r\n    ///     .load::<i32>(&conn)?;\r\n    /// assert_eq!(vec![1, 3], data);\r\n    ///\r\n    /// let data = posts.select(id)\r\n    ///     .filter(range.overlaps_with((Bound::Included(2), Bound::Included(5))))\r\n    ///     .load::<i32>(&conn)?;\r\n    /// assert_eq!(vec![1, 2, 3], data);\r\n    ///\r\n    /// let data = posts.select(id)\r\n    ///     .filter(range.overlaps_with((Bound::Included(1), Bound::Excluded(3))))\r\n    ///     .load::<i32>(&conn)?;\r\n    /// assert!(data.is_empty());\r\n    /// #     Ok(())\r\n    /// # }\r\n    /// ```\r\n    fn overlaps_with(self, other: R) -> OverlapsWith<Self, T>\r\n    {\r\n        OverlapsWith::new(self, other.as_expression())\r\n    }\r\n\r\n    /// Creates a PostgreSQL `@>` expression.\r\n    ///\r\n    /// This operator returns whether a range contains an element.\r\n    ///\r\n    /// # Example\r\n    ///\r\n    /// ```rust\r\n    /// # #[macro_use] extern crate diesel;\r\n    /// # include!(\"../../doctest_setup.rs\");\r\n    /// # use std::collections::Bound;\r\n    /// #\r\n    /// # table! {\r\n    /// #     posts {\r\n    /// #         id -> Integer,\r\n    /// #         range -> Range<Integer>,\r\n    /// #     }\r\n    /// # }\r\n    /// #\r\n    /// # fn main() {\r\n    /// #     run_test().unwrap();\r\n    /// # }\r\n    /// #\r\n    /// # fn run_test() -> QueryResult<()> {\r\n    /// #     use self::posts::dsl::*;\r\n    /// #     let conn = establish_connection();\r\n    /// #     conn.execute(\"DROP TABLE IF EXISTS posts\").unwrap();\r\n    /// #     conn.execute(\"CREATE TABLE posts (id SERIAL PRIMARY KEY, range INT4RANGE NOT NULL)\").unwrap();\r\n    /// #\r\n    /// diesel::insert_into(posts)\r\n    ///     .values(&vec![\r\n    ///         range.eq((Bound::Included(3), Bound::Included(5))),\r\n    ///         range.eq((Bound::Included(3), Bound::Excluded(5))),\r\n    ///         range.eq((Bound::Excluded(3), Bound::Unbounded)),\r\n    ///     ])\r\n    ///     .execute(&conn)?;\r\n    ///\r\n    /// let data = posts.select(id)\r\n    ///     .filter(range.contains(4))\r\n    ///     .load::<i32>(&conn)?;\r\n    /// assert_eq!(vec![1, 2, 3], data);\r\n    ///\r\n    /// #     Ok(())\r\n    /// # }\r\n    /// ```\r\n    #[allow(dead_code)]\r\n    fn contains(self, other: T) -> Contains<Self, T>\r\n    {\r\n        Contains::new(self, other.as_expression())\r\n    }\r\n}\r\n\r\n#[doc(hidden)]\r\n/// Marker trait used to implement `RangeExpressionMethods` on the appropriate\r\n/// types. Once coherence takes associated types into account, we can remove\r\n/// this trait.\r\npub trait RangeOrNullableRange {}\r\n\r\nimpl<T> RangeOrNullableRange for Range<T> {}\r\nimpl<T> RangeOrNullableRange for Nullable<Range<T>> {}\r\n\r\nimpl<T,R> PgRangeExpressionMethods<R> for T\r\nwhere\r\n    T: Expression,\r\n    T::SqlType: RangeOrNullableRange,\r\n    R: Expression,\r\n{\r\n}\r\n```\r\n\r\nbut it's not compiling and my rust-fu isn't good enough. Have played around with quite a few different ways. Any help would be great!","author_association":"CONTRIBUTOR","reactions":{"url":"https://api.github.com/repos/diesel-rs/diesel/issues/comments/546623788/reactions","total_count":0,"+1":0,"-1":0,"laugh":0,"hooray":0,"confused":0,"heart":0,"rocket":0,"eyes":0},"performed_via_github_app":null,"event":"commented","actor":{"login":"cazgp","id":658873,"node_id":"MDQ6VXNlcjY1ODg3Mw==","avatar_url":"https://avatars.githubusercontent.com/u/658873?v=4","gravatar_id":"","url":"https://api.github.com/users/cazgp","html_url":"https://github.com/cazgp","followers_url":"https://api.github.com/users/cazgp/followers","following_url":"https://api.github.com/users/cazgp/following{/other_user}","gists_url":"https://api.github.com/users/cazgp/gists{/gist_id}","starred_url":"https://api.github.com/users/cazgp/starred{/owner}{/repo}","subscriptions_url":"https://api.github.com/users/cazgp/subscriptions","organizations_url":"https://api.github.com/users/cazgp/orgs","repos_url":"https://api.github.com/users/cazgp/repos","events_url":"https://api.github.com/users/cazgp/events{/privacy}","received_events_url":"https://api.github.com/users/cazgp/received_events","type":"User","user_view_type":"public","site_admin":false}},{"url":"https://api.github.com/repos/diesel-rs/diesel/issues/comments/546623900","html_url":"https://github.com/diesel-rs/diesel/issues/2210#issuecomment-546623900","issue_url":"https://api.github.com/repos/diesel-rs/diesel/issues/2210","id":546623900,"node_id":"MDEyOklzc3VlQ29tbWVudDU0NjYyMzkwMA==","user":{"login":"cazgp","id":658873,"node_id":"MDQ6VXNlcjY1ODg3Mw==","avatar_url":"https://avatars.githubusercontent.com/u/658873?v=4","gravatar_id":"","url":"https://api.github.com/users/cazgp","html_url":"https://github.com/cazgp","followers_url":"https://api.github.com/users/cazgp/followers","following_url":"https://api.github.com/users/cazgp/following{/other_user}","gists_url":"https://api.github.com/users/cazgp/gists{/gist_id}","starred_