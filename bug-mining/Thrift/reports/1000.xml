<item>
            <title>[THRIFT-1609] Cocoa import namespace</title>
                <link>https://issues.apache.org/jira/browse/THRIFT-1609</link>
                <project id="12310800" key="THRIFT">Thrift</project>
                    <description>&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeHeader panelHeader&quot; style=&quot;border-bottom-width: 1px;&quot;&gt;&lt;b&gt;domain.thrift&lt;/b&gt;&lt;/div&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt;namespace java domain.thrift
namespace cocoa domain

typedef string Decimal
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;

&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeHeader panelHeader&quot; style=&quot;border-bottom-width: 1px;&quot;&gt;&lt;b&gt;AccountService.thrift&lt;/b&gt;&lt;/div&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt;include &lt;span class=&quot;code-quote&quot;&gt;&quot;domain.thrift&quot;&lt;/span&gt;

namespace java test
namespace cocoa test

service AccountService {
  void testMethod(1: domain.Decimal price),
}
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;

&lt;p&gt;thrift -r --gen cocoa domain.thrift&lt;br/&gt;
thrift -r --gen cocoa AccountService.thrift&lt;/p&gt;

&lt;p&gt;Check file: gen-cocoa/AccountService.h&lt;/p&gt;

&lt;p&gt;line:&lt;/p&gt;
&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt;@protocol testAccountService &amp;lt;NSObject&amp;gt;
- (void) testMethod: (testDecimal) price;  &lt;span class=&quot;code-comment&quot;&gt;// &lt;span class=&quot;code-keyword&quot;&gt;throws&lt;/span&gt; TException
&lt;/span&gt;@end
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;

&lt;p&gt;It should be:&lt;/p&gt;
&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt;@protocol testAccountService &amp;lt;NSObject&amp;gt;
- (void) testMethod: (domainDecimal) price;  &lt;span class=&quot;code-comment&quot;&gt;// &lt;span class=&quot;code-keyword&quot;&gt;throws&lt;/span&gt; TException
&lt;/span&gt;@end
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;</description>
                <environment>&lt;p&gt;Windows 7&lt;/p&gt;</environment>
