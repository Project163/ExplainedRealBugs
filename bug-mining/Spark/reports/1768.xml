            <title>[SPARK-7229] SpecificMutableRow should take integer type as internal representation for DateType</title>
                <link>https://issues.apache.org/jira/browse/SPARK-7229</link>
                <project id="12315420" key="SPARK">Spark</project>
                    <description>&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt;  test(&lt;span class=&quot;code-quote&quot;&gt;&quot;test DATE types in cache&quot;&lt;/span&gt;) {
    val rows = TestSQLContext.jdbc(urlWithUserAndPass, &lt;span class=&quot;code-quote&quot;&gt;&quot;TEST.TIMETYPES&quot;&lt;/span&gt;).collect()
    TestSQLContext.jdbc(urlWithUserAndPass, &lt;span class=&quot;code-quote&quot;&gt;&quot;TEST.TIMETYPES&quot;&lt;/span&gt;).cache().registerTempTable(&lt;span class=&quot;code-quote&quot;&gt;&quot;mycached_date&quot;&lt;/span&gt;)
    val cachedRows = sql(&lt;span class=&quot;code-quote&quot;&gt;&quot;select * from mycached_date&quot;&lt;/span&gt;).collect()
    &lt;span class=&quot;code-keyword&quot;&gt;assert&lt;/span&gt;(rows(0).getAs[java.sql.Date](1) === java.sql.Date.valueOf(&lt;span class=&quot;code-quote&quot;&gt;&quot;1996-01-01&quot;&lt;/span&gt;))
    &lt;span class=&quot;code-keyword&quot;&gt;assert&lt;/span&gt;(cachedRows(0).getAs[java.sql.Date](1) === java.sql.Date.valueOf(&lt;span class=&quot;code-quote&quot;&gt;&quot;1996-01-01&quot;&lt;/span&gt;))
  }
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;
