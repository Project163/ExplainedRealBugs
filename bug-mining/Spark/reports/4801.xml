<item>
            <title>[SPARK-19965] DataFrame batch reader may fail to infer partitions when reading FileStreamSink&apos;s output</title>
                <link>https://issues.apache.org/jira/browse/SPARK-19965</link>
                <project id="12315420" key="SPARK">Spark</project>
                    <description>&lt;p&gt;Reproducer&lt;/p&gt;
&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt;  test(&lt;span class=&quot;code-quote&quot;&gt;&quot;partitioned writing and batch reading with &lt;span class=&quot;code-quote&quot;&gt;&apos;basePath&apos;&lt;/span&gt;&quot;&lt;/span&gt;) {
    val inputData = MemoryStream[Int]
    val ds = inputData.toDS()

    val outputDir = Utils.createTempDir(namePrefix = &lt;span class=&quot;code-quote&quot;&gt;&quot;stream.output&quot;&lt;/span&gt;).getCanonicalPath
    val checkpointDir = Utils.createTempDir(namePrefix = &lt;span class=&quot;code-quote&quot;&gt;&quot;stream.checkpoint&quot;&lt;/span&gt;).getCanonicalPath

    &lt;span class=&quot;code-keyword&quot;&gt;var&lt;/span&gt; query: StreamingQuery = &lt;span class=&quot;code-keyword&quot;&gt;null&lt;/span&gt;

    &lt;span class=&quot;code-keyword&quot;&gt;try&lt;/span&gt; {
      query =
        ds.map(i =&amp;gt; (i, i * 1000))
          .toDF(&lt;span class=&quot;code-quote&quot;&gt;&quot;id&quot;&lt;/span&gt;, &lt;span class=&quot;code-quote&quot;&gt;&quot;value&quot;&lt;/span&gt;)
          .writeStream
          .partitionBy(&lt;span class=&quot;code-quote&quot;&gt;&quot;id&quot;&lt;/span&gt;)
          .option(&lt;span class=&quot;code-quote&quot;&gt;&quot;checkpointLocation&quot;&lt;/span&gt;, checkpointDir)
          .format(&lt;span class=&quot;code-quote&quot;&gt;&quot;parquet&quot;&lt;/span&gt;)
          .start(outputDir)

      inputData.addData(1, 2, 3)
      failAfter(streamingTimeout) {
        query.processAllAvailable()
      }

      spark.read.option(&lt;span class=&quot;code-quote&quot;&gt;&quot;basePath&quot;&lt;/span&gt;, outputDir).parquet(outputDir + &lt;span class=&quot;code-quote&quot;&gt;&quot;/*&quot;&lt;/span&gt;).show()
    } &lt;span class=&quot;code-keyword&quot;&gt;finally&lt;/span&gt; {
      &lt;span class=&quot;code-keyword&quot;&gt;if&lt;/span&gt; (query != &lt;span class=&quot;code-keyword&quot;&gt;null&lt;/span&gt;) {
        query.stop()
      }
    }
  }
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;

&lt;p&gt;Stack trace&lt;/p&gt;
