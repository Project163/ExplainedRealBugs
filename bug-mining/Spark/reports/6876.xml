+-- !query 47 output
+[1582-10-03,1582-10-04,1582-10-15,&lt;span class=&quot;code-keyword&quot;&gt;null&lt;/span&gt;]
+
+
+-- !query 48
+select &lt;span class=&quot;code-keyword&quot;&gt;cast&lt;/span&gt;(&lt;span class=&quot;code-quote&quot;&gt;&apos;1582-10-13&apos;&lt;/span&gt; as date), date &lt;span class=&quot;code-quote&quot;&gt;&apos;1582-10-14&apos;&lt;/span&gt;, date &lt;span class=&quot;code-quote&quot;&gt;&apos;1582-10-15&apos;&lt;/span&gt;
+-- !query 48 schema
+struct&amp;lt;CAST(1582-10-13 AS DATE):date,DATE &lt;span class=&quot;code-quote&quot;&gt;&apos;1582-10-14&apos;&lt;/span&gt;:date,DATE &lt;span class=&quot;code-quote&quot;&gt;&apos;1582-10-15&apos;&lt;/span&gt;:date&amp;gt;
+-- !query 48 output
+1582-10-13     1582-10-14      1582-10-15
&lt;/pre&gt;
