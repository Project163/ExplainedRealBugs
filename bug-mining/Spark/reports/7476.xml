  &lt;span class=&quot;code-quote&quot;&gt;&quot;class&quot;&lt;/span&gt; : &lt;span class=&quot;code-quote&quot;&gt;&quot;org.apache.spark.sql.catalyst.expressions.AttributeReference&quot;&lt;/span&gt;,
