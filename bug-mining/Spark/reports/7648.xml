val df = spark.range(1, 100)
df.createTempView(&lt;span class=&quot;code-quote&quot;&gt;&quot;df&quot;&lt;/span&gt;)
spark.sql(&lt;span class=&quot;code-quote&quot;&gt;&quot;SELECT (SELECT min(id) AS v FROM df)&quot;&lt;/span&gt;).explain(&lt;span class=&quot;code-quote&quot;&gt;&quot;CODEGEN&quot;&lt;/span&gt;)

scala&amp;gt; spark.sql(&lt;span class=&quot;code-quote&quot;&gt;&quot;SELECT (SELECT min(id) AS v FROM df)&quot;&lt;/span&gt;).explain(&lt;span class=&quot;code-quote&quot;&gt;&quot;CODEGEN&quot;&lt;/span&gt;)
