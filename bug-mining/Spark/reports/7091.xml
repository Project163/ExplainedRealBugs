/* This is the first example of bracketed comment.
SELECT &lt;span class=&quot;code-quote&quot;&gt;&apos;ommented out content&apos;&lt;/span&gt; AS first;
*/
SELECT &lt;span class=&quot;code-quote&quot;&gt;&apos;selected content&apos;&lt;/span&gt; AS first;
--QUERY-DELIMITER-END
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;
&lt;p&gt;The test case &lt;/p&gt;
&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt;
comments.sql 
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;
&lt;p&gt; imports &lt;/p&gt;
&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt;
nested-comments.sql
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;
&lt;p&gt; below:&lt;/p&gt;

&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt;
--IMPORT nested-comments.sql
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;

