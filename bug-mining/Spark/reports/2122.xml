        at org.apache.spark.scheduler.TaskSchedulerImpl.start(TaskSchedulerImpl.scala:141)&lt;br/&gt;
        at org.apache.spark.SparkContext.&amp;lt;init&amp;gt;(SparkContext.scala:497)&lt;br/&gt;
        at org.apache.spark.repl.SparkILoop.createSparkContext(SparkILoop.scala:1017)&lt;br/&gt;
        at $line3.$read$$iwC$$iwC.&amp;lt;init&amp;gt;(&amp;lt;console&amp;gt;:9)&lt;br/&gt;
        at $line3.$read$$iwC.&amp;lt;init&amp;gt;(&amp;lt;console&amp;gt;:18)&lt;br/&gt;
        at $line3.$read.&amp;lt;init&amp;gt;(&amp;lt;console&amp;gt;:20)&lt;br/&gt;
