        at org.apache.spark.deploy.yarn.Client$$anonfun$prepareLocalResources$5.apply(Client.scala:341)&lt;br/&gt;
        at org.apache.spark.deploy.yarn.Client$$anonfun$prepareLocalResources$5.apply(Client.scala:338)&lt;br/&gt;
        at scala.Option.foreach(Option.scala:236)&lt;br/&gt;
