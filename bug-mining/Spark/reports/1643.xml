df0.save(&lt;span class=&quot;code-quote&quot;&gt;&quot;/tmp/df0&quot;&lt;/span&gt;) &lt;span class=&quot;code-comment&quot;&gt;// works
&lt;/span&gt;val df1 = sqlContext.createDataFrame(df0.rdd, df0.schema)
df1.save(&lt;span class=&quot;code-quote&quot;&gt;&quot;/tmp/df1&quot;&lt;/span&gt;) &lt;span class=&quot;code-comment&quot;&gt;// error&lt;/span&gt;
