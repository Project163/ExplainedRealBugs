            <title>[SPARK-3349] Incorrect partitioning after LIMIT operator</title>
                <link>https://issues.apache.org/jira/browse/SPARK-3349</link>
                <project id="12315420" key="SPARK">Spark</project>
                    <description>&lt;p&gt;Reproduced by the following example:&lt;/p&gt;
&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt;&lt;span class=&quot;code-keyword&quot;&gt;import&lt;/span&gt; org.apache.spark.sql.catalyst.plans.Inner
&lt;span class=&quot;code-keyword&quot;&gt;import&lt;/span&gt; org.apache.spark.sql.catalyst.analysis.UnresolvedAttribute
&lt;span class=&quot;code-keyword&quot;&gt;import&lt;/span&gt; org.apache.spark.sql.catalyst.expressions._

val series = sql(&lt;span class=&quot;code-quote&quot;&gt;&quot;select distinct year from sales order by year asc limit 10&quot;&lt;/span&gt;)
val results = sql(&lt;span class=&quot;code-quote&quot;&gt;&quot;select * from sales&quot;&lt;/span&gt;)
series.registerTempTable(&lt;span class=&quot;code-quote&quot;&gt;&quot;series&quot;&lt;/span&gt;)
results.registerTempTable(&lt;span class=&quot;code-quote&quot;&gt;&quot;results&quot;&lt;/span&gt;)

sql(&lt;span class=&quot;code-quote&quot;&gt;&quot;select * from results &lt;span class=&quot;code-keyword&quot;&gt;inner&lt;/span&gt; join series where results.year = series.year&quot;&lt;/span&gt;).count

