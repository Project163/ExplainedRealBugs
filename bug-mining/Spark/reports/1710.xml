 table &lt;span class=&quot;code-quote&quot;&gt;&quot;test1&quot;&lt;/span&gt;,
 keyspace &lt;span class=&quot;code-quote&quot;&gt;&quot;test&quot;&lt;/span&gt;
)
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt; 

