	&lt;li&gt;scala.tools.nsc.interpreter.AbstractFileClassLoader -&amp;gt; scala.reflect.internal.util.AbstractFileClassLoader&lt;/li&gt;
	&lt;li&gt;TypeApi.declarations-&amp;gt; decls&lt;/li&gt;
&lt;/ul&gt;
