&lt;span class=&quot;code-comment&quot;&gt;/* 080 */&lt;/span&gt;         serializefromobject_rowWriter.setNullAt(0);
&lt;span class=&quot;code-comment&quot;&gt;/* 081 */&lt;/span&gt;       } &lt;span class=&quot;code-keyword&quot;&gt;else&lt;/span&gt; {
&lt;span class=&quot;code-comment&quot;&gt;/* 082 */&lt;/span&gt;         serializefromobject_rowWriter.write(0, serializefromobject_value);
&lt;span class=&quot;code-comment&quot;&gt;/* 083 */&lt;/span&gt;       }
