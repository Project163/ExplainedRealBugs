&lt;span class=&quot;code-comment&quot;&gt;/* 071 */&lt;/span&gt;         serializefromobject_isNull6 = serializefromobject_value6 == &lt;span class=&quot;code-keyword&quot;&gt;null&lt;/span&gt;;
&lt;span class=&quot;code-comment&quot;&gt;/* 072 */&lt;/span&gt;         serializefromobject_isNull = serializefromobject_isNull6;
