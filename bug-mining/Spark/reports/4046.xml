&lt;span class=&quot;code-comment&quot;&gt;/* 026 */&lt;/span&gt;   }
&lt;span class=&quot;code-comment&quot;&gt;/* 027 */&lt;/span&gt;
&lt;span class=&quot;code-comment&quot;&gt;/* 028 */&lt;/span&gt;   &lt;span class=&quot;code-keyword&quot;&gt;protected&lt;/span&gt; void processNext() &lt;span class=&quot;code-keyword&quot;&gt;throws&lt;/span&gt; java.io.IOException {
&lt;span class=&quot;code-comment&quot;&gt;/* 029 */&lt;/span&gt;     &lt;span class=&quot;code-keyword&quot;&gt;while&lt;/span&gt; (inputadapter_input.hasNext()) {
&lt;span class=&quot;code-comment&quot;&gt;/* 030 */&lt;/span&gt;       InternalRow inputadapter_row = (InternalRow) inputadapter_input.next();
&lt;span class=&quot;code-comment&quot;&gt;/* 031 */&lt;/span&gt;       org.apache.spark.sql.Row inputadapter_value = (org.apache.spark.sql.Row)inputadapter_row.get(0, &lt;span class=&quot;code-keyword&quot;&gt;null&lt;/span&gt;);
&lt;span class=&quot;code-comment&quot;&gt;/* 032 */&lt;/span&gt;
&lt;span class=&quot;code-comment&quot;&gt;/* 033 */&lt;/span&gt;       &lt;span class=&quot;code-keyword&quot;&gt;if&lt;/span&gt; (&lt;span class=&quot;code-keyword&quot;&gt;false&lt;/span&gt;) {
&lt;span class=&quot;code-comment&quot;&gt;/* 034 */&lt;/span&gt;         &lt;span class=&quot;code-keyword&quot;&gt;throw&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;new&lt;/span&gt; RuntimeException(serializefromobject_errMsg);
