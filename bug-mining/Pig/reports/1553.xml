    at org.apache.pig.PigServer.executeCompiledLogicalPlan(PigServer.java:1307)&lt;br/&gt;
    at org.apache.pig.PigServer.execute(PigServer.java:1297)&lt;br/&gt;
    at org.apache.pig.PigServer.executeBatch(PigServer.java:375)&lt;br/&gt;
    at org.apache.pig.PigServer.executeBatch(PigServer.java:353)&lt;br/&gt;
    at&lt;br/&gt;
org.apache.pig.tools.grunt.GruntParser.executeBatch(GruntParser.java:140)&lt;br/&gt;
    at&lt;br/&gt;
org.apache.pig.tools.grunt.GruntParser.parseStopOnError(GruntParser.java:202)&lt;br/&gt;
