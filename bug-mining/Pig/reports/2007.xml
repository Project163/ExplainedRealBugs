&#160; &#160; | &#160; |
&#160; &#160; | &#160; (Name: LOGenerate[&lt;span class=&quot;code-keyword&quot;&gt;false&lt;/span&gt;,&lt;span class=&quot;code-keyword&quot;&gt;false&lt;/span&gt;,&lt;span class=&quot;code-keyword&quot;&gt;false&lt;/span&gt;,&lt;span class=&quot;code-keyword&quot;&gt;false&lt;/span&gt;] Schema: name1#121:chararray,name2#122:chararray,age#105:&lt;span class=&quot;code-object&quot;&gt;int&lt;/span&gt;,gpr#106:&lt;span class=&quot;code-object&quot;&gt;float&lt;/span&gt;)ColumnPrune:OutputUids=[121, 105, 122, 106]ColumnPrune:InputUids=[121, 105, 122, 106]
