                        <labels>
                    </labels>
                <created>Wed, 5 Sep 2012 03:12:45 +0000</created>
                <updated>Thu, 11 Oct 2012 17:45:10 +0000</updated>
                            <resolved>Wed, 5 Sep 2012 04:41:46 +0000</resolved>
                                    <version>2.0.2-alpha</version>
                                    <fixVersion>2.0.2-alpha</fixVersion>
                                    <component>build</component>
