       &lt;span class=&quot;code-object&quot;&gt;System&lt;/span&gt;.out.println(chgrp);
-    } &lt;span class=&quot;code-keyword&quot;&gt;else&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;if&lt;/span&gt; (Count.NAME.equals(cmd)) {
+    } &lt;span class=&quot;code-keyword&quot;&gt;else&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;if&lt;/span&gt; (Count.matches(cmd)) {
       &lt;span class=&quot;code-object&quot;&gt;System&lt;/span&gt;.out.println(Count.DESCRIPTION);
     } &lt;span class=&quot;code-keyword&quot;&gt;else&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;if&lt;/span&gt; (&lt;span class=&quot;code-quote&quot;&gt;&quot;help&quot;&lt;/span&gt;.equals(cmd)) {
