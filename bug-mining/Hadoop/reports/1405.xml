&lt;pre class=&quot;code-java&quot;&gt;#!/bin/bash

function printValue(){
  echo &lt;span class=&quot;code-quote&quot;&gt;&quot;Printing args : $@&quot;&lt;/span&gt;
}

export -f printValue

echo &lt;span class=&quot;code-quote&quot;&gt;&quot;arg1 arg2&quot;&lt;/span&gt; | 
 xargs -n 1 -P 10 -I {} bash -c -- &lt;span class=&quot;code-quote&quot;&gt;&apos;printValue {} remaining args&apos;&lt;/span&gt;&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;

