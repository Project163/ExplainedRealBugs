&lt;span class=&quot;code-comment&quot;&gt;// HERE is where the IOException &lt;span class=&quot;code-keyword&quot;&gt;throws&lt;/span&gt;
&lt;/span&gt;    &lt;span class=&quot;code-keyword&quot;&gt;throw&lt;/span&gt; ioe;
