              &amp;lt;execution&amp;gt;
                &amp;lt;id&amp;gt;pre-site&amp;lt;/id&amp;gt;
                &amp;lt;phase&amp;gt;prepare-&lt;span class=&quot;code-keyword&quot;&gt;package&lt;/span&gt;&amp;lt;/phase&amp;gt;
                &amp;lt;goals&amp;gt;
                  &amp;lt;goal&amp;gt;run&amp;lt;/goal&amp;gt;
                &amp;lt;/goals&amp;gt;
                &amp;lt;configuration&amp;gt;
                  &amp;lt;target &lt;span class=&quot;code-keyword&quot;&gt;if&lt;/span&gt;=&lt;span class=&quot;code-quote&quot;&gt;&quot;is.hadoop.common.component&quot;&lt;/span&gt;&amp;gt;
                    &amp;lt;!-- HADOOP-13428: This is &lt;span class=&quot;code-keyword&quot;&gt;for&lt;/span&gt; workaround issues of JDiff
                         - https:&lt;span class=&quot;code-comment&quot;&gt;//sourceforge.net/p/javadiff/bugs/19/ --&amp;gt;
&lt;/span&gt;                    &amp;lt;exec executable=&lt;span class=&quot;code-quote&quot;&gt;&quot;patch&quot;&lt;/span&gt; input=&lt;span class=&quot;code-quote&quot;&gt;&quot;dev-support/jdiff-workaround.patch&quot;&lt;/span&gt; dir=&lt;span class=&quot;code-quote&quot;&gt;&quot;${basedir}&quot;&lt;/span&gt;&amp;gt;
                        &amp;lt;arg value=&lt;span class=&quot;code-quote&quot;&gt;&quot;-p3&quot;&lt;/span&gt;/&amp;gt;
                    &amp;lt;/exec&amp;gt;
                  &amp;lt;/target&amp;gt;
                &amp;lt;/configuration&amp;gt;
              &amp;lt;/execution&amp;gt;
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;</comment>
