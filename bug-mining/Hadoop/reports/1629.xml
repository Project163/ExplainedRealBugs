printStream.println(&lt;br/&gt;
    &quot;    SSD_QUOTA     REM_SSD_QUOTA    DISK_QUOTA    REM_DISK_QUOTA ARCHIVE_QUOTA REM_ARCHIVE_QUOTA PATHNAME&quot;&lt;br/&gt;
);&lt;br/&gt;
-&amp;gt; at org.apache.hadoop.fs.shell.Count.processOptions(Count.java:124)&lt;/p&gt;

