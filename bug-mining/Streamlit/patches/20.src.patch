-export const getDimensions = (
-  height: number | undefined,
-  width: number,
-  element: Quiver,
-  cellContentsGetter: CellContentsGetter
-): Dimensions => {
-  const {
-    headerRows,
-    headerColumns,
-    dataRows,
-    columns,
