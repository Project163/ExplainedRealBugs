-          CellType={Component}
-          id={cssId}
-          className={cssClass}
-          columnIndex={columnIndex}
-          rowIndex={rowIndex}
-          style={style}
-          isNumeric={isNumeric}
-          contents={contents}
-          sortedByUser={sortedByUser}
-          columnSortDirection={columnSortDirection}
-          headerClickedCallback={headerClickedCallback}
-        />
-      )
-    }
-  }
