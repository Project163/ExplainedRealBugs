+    }
+  })
+
+  return {
+    individualPages,
+    sections,
+  }
+}
+
+/**
+ * Helper to get all pages in display order (individuals first, then sections)
+ */
+export function getAllPagesInOrder(
+  processed: ProcessedNavigation
+): IAppPage[] {
+  return [
+    ...processed.individualPages,
+    ...Object.values(processed.sections).flat(),
+  ]
+}
