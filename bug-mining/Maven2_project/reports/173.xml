+                    String match = m.group();&lt;br/&gt;
+                    String key = match.substring(2, match.length()-1);&lt;br/&gt;
+                    String value = properties.getProperty(key);&lt;/p&gt;

&lt;p&gt;+                  if (value == null) &lt;/p&gt;
{

+                         System.out.println(&quot;Trying .. system wide property..&quot;);

+                         value = System.getProperty(key);

+                         if (value !=null) 

+                             System.out.println(&quot;Found value from sys=&quot; + value);

+                   }

&lt;p&gt;+                    if (value != null) {&lt;br/&gt;
+                        s = s.replaceAll(&quot;\\$&lt;br class=&quot;atl-forced-newline&quot; /&gt;&lt;/p&gt;
{&quot; + key + &quot;\\}
&lt;p&gt;&quot;, value);&lt;br/&gt;
+                    }&lt;/p&gt;</comment>
