+  { types: t }: { types: Object },
+  { include, exclude, polyfillTargets, debug }: InternalPluginOptions,
+) {
+  const polyfills = filterItems(
+    corejs2Polyfills,
+    include,
+    exclude,
+    polyfillTargets,
+    getPlatformSpecificDefaultFor(polyfillTargets),
+  );
