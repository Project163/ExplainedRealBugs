\n[[language]]\nname = \"scss\"\nauto-format = true\nformatter = { command = \"prettier\", args = [\"--parser\", \"scss\" ] }\n\n[[language]]\nname = \"yaml\"\nauto-format = true\nformatter = { command = \"prettier\", args = [\"--parser\", \"yaml\" ] }\n\n[[language]]\nname = \"markdown\"\nauto-format = true\nformatter = { command = \"prettier\", args = [\"--parser\", \"markdown\" ] }\n\n[language-server.typescript-language-server]\ncommand = \"typescript-language-server\"\nargs = [\"--stdio\"]\nconfig.hostInfo = \"helix\"\nconfig.preferences.importModuleSpecifierPreference = \"non-relative\"\nconfig.tsserver.path = \"node_modules/typescript/lib/tsserver.js\"\n\n[language-server.angular-language-server]\ncommand = \"ngserver\"\nargs = [\n  \"--stdio\",\n  \"--tsProbeLocations\", \"node_modules\",\n  \"--ngProbeLocations\", \"node_modules/@angular/language-server\"\n]\n```\n","author_association":"NONE","reactions":{"url":"https://api.github.com/repos/helix-editor/helix/issues/comments/2889851327/reactions","total_count":0,"+1":0,"-1":0,"laugh":0,"hooray":0,"confused":0,"heart":0,"rocket":0,"eyes":0},"performed_via_github_app":null,"event":"commented","actor":{"login":"danriedl","id":107212962,"node_id":"U_kgDOBmPwog","avatar_url":"https://avatars.githubusercontent.com/u/107212962?v=4","gravatar_id":"","url":"https://api.github.com/users/danriedl","html_url":"https://github.com/danriedl","followers_url":"https://api.github.com/users/danriedl/followers","following_url":"https://api.github.com/users/danriedl/following{/other_user}","gists_url":"https://api.github.com/users/danriedl/gists{/gist_id}","starred_url":"https://api.github.com/users/danriedl/starred{/owner}{/repo}","subscriptions_url":"https://api.github.com/users/danriedl/subscriptions","organizations_url":"https://api.github.com/users/danriedl/orgs","repos_url":"https://api.github.com/users/danriedl/repos","events_url":"https://api.github.com/users/danriedl/events{/privacy}","received_events_url":"https://ap