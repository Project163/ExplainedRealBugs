&lt;ul&gt;
	&lt;li&gt;moveFile()&lt;/li&gt;
	&lt;li&gt;moveFileToDirectory()&lt;/li&gt;
	&lt;li&gt;moveDirectory()&lt;/li&gt;
	&lt;li&gt;moveToDirectory()&lt;/li&gt;
	&lt;li&gt;moveDirectoryToDirectory()&lt;/li&gt;
&lt;/ul&gt;


