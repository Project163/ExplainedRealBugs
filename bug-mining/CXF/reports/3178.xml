        &lt;span class=&quot;code-keyword&quot;&gt;public&lt;/span&gt; T set(T transfer) {
            &lt;span class=&quot;code-keyword&quot;&gt;return&lt;/span&gt; transfer;
        }

    }
    
    @XmlRootElement(namespace = &lt;span class=&quot;code-quote&quot;&gt;&quot;http:&lt;span class=&quot;code-comment&quot;&gt;//example.com&quot;&lt;/span&gt;)
&lt;/span&gt;    &lt;span class=&quot;code-keyword&quot;&gt;public&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;static&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;class &lt;/span&gt;Actual &lt;span class=&quot;code-keyword&quot;&gt;extends&lt;/span&gt; Super{}
    
    &lt;span class=&quot;code-keyword&quot;&gt;public&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;static&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;class &lt;/span&gt;ActualResource &lt;span class=&quot;code-keyword&quot;&gt;extends&lt;/span&gt; SuperResource&amp;lt;Actual&amp;gt; {}
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;

