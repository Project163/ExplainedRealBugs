    &amp;lt;/wsdl:operation&amp;gt;&lt;br/&gt;
  &amp;lt;/wsdl:binding&amp;gt;&lt;br/&gt;
  &amp;lt;wsdl:service name=&quot;PicketLinkSTS&quot;&amp;gt;&lt;br/&gt;
    &amp;lt;wsdl:port name=&quot;PicketLinkSTSPort&quot; binding=&quot;tns:STSBinding&quot;&amp;gt;&lt;br/&gt;
      &amp;lt;soap12:address location=&quot;http://localhost:8080/picketlink-sts&quot;/&amp;gt;&lt;br/&gt;
    &amp;lt;/wsdl:port&amp;gt;&lt;br/&gt;
