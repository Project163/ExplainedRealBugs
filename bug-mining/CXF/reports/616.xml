    xmlns:wsdl=&quot;http://schemas.xmlsoap.org/wsdl/&quot; &lt;br/&gt;
    xmlns:wsap10=&quot;http://www.w3.org/2006/05/addressing/wsdl&quot;&lt;br/&gt;
    xmlns:soap12=&quot;http://schemas.xmlsoap.org/wsdl/soap12/&quot;&amp;gt;&lt;br/&gt;
