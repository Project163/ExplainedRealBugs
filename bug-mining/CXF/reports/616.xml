    &amp;lt;wsdl:operation name=&quot;IssueToken&quot;&amp;gt;&lt;br/&gt;
      &amp;lt;soap12:operation soapAction=&quot;http://schemas.xmlsoap.org/ws/2005/02/trust/RST/Issue&quot; style=&quot;document&quot;/&amp;gt;&lt;br/&gt;
      &amp;lt;wsdl:input&amp;gt;&lt;br/&gt;
        &amp;lt;soap12:body use=&quot;literal&quot;/&amp;gt;&lt;br/&gt;
