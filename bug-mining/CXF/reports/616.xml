      xmlns:xs=&quot;http://www.w3.org/2001/XMLSchema&quot;&lt;br/&gt;
      version=&quot;1.0&quot;&amp;gt;&lt;br/&gt;
      &amp;lt;xs:element  name=&quot;MessageBody&quot;&amp;gt;&lt;br/&gt;
