  &amp;lt;/wsdl:message&amp;gt;&lt;br/&gt;
  &amp;lt;wsdl:portType name=&quot;SecureTokenService&quot;&amp;gt;&lt;br/&gt;
    &amp;lt;wsdl:operation name=&quot;IssueToken&quot;&amp;gt;&lt;br/&gt;
      &amp;lt;wsdl:input wsap10:Action=&quot;http://schemas.xmlsoap.org/ws/2005/02/trust/RST/Issue&quot; message=&quot;tns:RequestSecurityToken&quot;/&amp;gt;&lt;br/&gt;
      &amp;lt;wsdl:output wsap10:Action=&quot;http://schemas.xmlsoap.org/ws/2005/02/trust/RSTR/Issue&quot; message=&quot;tns:RequestSecurityTokenResponse&quot;/&amp;gt;&lt;br/&gt;
    &amp;lt;/wsdl:operation&amp;gt;&lt;br/&gt;
  &amp;lt;/wsdl:portType&amp;gt;&lt;br/&gt;
  &amp;lt;wsdl:binding name=&quot;STSBinding&quot; type=&quot;tns:SecureTokenService&quot;&amp;gt;&lt;br/&gt;
