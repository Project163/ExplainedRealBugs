&lt;span class=&quot;code-tag&quot;&gt;&amp;lt;SOAP-ENV:Header &lt;span class=&quot;code-keyword&quot;&gt;xmlns:SOAP-ENV&lt;/span&gt;=&lt;span class=&quot;code-quote&quot;&gt;&quot;http://www.w3.org/2003/05/soap-envelope&quot;&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;xmlns:SOAP-ENC&lt;/span&gt;=&lt;span class=&quot;code-quote&quot;&gt;&quot;http://www.w3.org/2003/05/soap-encoding&quot;&lt;/span&gt;&amp;gt;&lt;/span&gt;&lt;span class=&quot;code-tag&quot;&gt;&amp;lt;/SOAP-ENV:Header&amp;gt;&lt;/span&gt;&lt;span class=&quot;code-tag&quot;&gt;&amp;lt;SOAP-ENV:Body&amp;gt;&lt;/span&gt;
&lt;span class=&quot;code-tag&quot;&gt;&amp;lt;xsd1:statusCode &lt;span class=&quot;code-keyword&quot;&gt;xmlns:xsd1&lt;/span&gt;=&lt;span class=&quot;code-quote&quot;&gt;&quot;http://www.xxx.yy/zzz/types/&quot;&lt;/span&gt;&amp;gt;&lt;/span&gt;-1&lt;span class=&quot;code-tag&quot;&gt;&amp;lt;/xsd1:statusCode&amp;gt;&lt;/span&gt;
  &lt;span class=&quot;code-tag&quot;&gt;&amp;lt;xsd1:statusMessage &lt;span class=&quot;code-keyword&quot;&gt;xmlns:xsd1&lt;/span&gt;=&lt;span class=&quot;code-quote&quot;&gt;&quot;http://www.xxx.yy/zzz/types/&quot;&lt;/span&gt;&amp;gt;&lt;/span&gt;Some Status Message&lt;span class=&quot;code-tag&quot;&gt;&amp;lt;/xsd1:statusMessage&amp;gt;&lt;/span&gt;
  &lt;span class=&quot;code-tag&quot;&gt;&amp;lt;xsd1:pdfList &lt;span class=&quot;code-keyword&quot;&gt;xmlns:xsd1&lt;/span&gt;=&lt;span class=&quot;code-quote&quot;&gt;&quot;http://www.xxx.yy/zzz/types/&quot;&lt;/span&gt;&amp;gt;&lt;/span&gt;&lt;span class=&quot;code-tag&quot;&gt;&amp;lt;/xsd1:pdfList&amp;gt;&lt;/span&gt;
