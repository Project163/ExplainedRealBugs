+            synchronized (this.mapping) {
+                if ((this.objects == null || this.objects.isEmpty())
+                    && this.hasMore) {
+                    try {
+                        this.fetch();
+                    } catch (IOException ex) {
+                        throw new IllegalStateException(ex);
+                    }
+                }
+                return !this.objects.isEmpty();
+            }
+        }
