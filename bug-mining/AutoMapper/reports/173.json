\n    public class A { }\r\n    \r\n    public class B { }\r\n\r\n    public class C\r\n    {\r\n        public string property1 { get; set; }\r\n    }\r\n    \r\n    public class MyTestResolver :\r\n        IValueResolver<A, C, string>,\r\n        IValueResolver<B, C, string>\r\n    {\r\n        public string Resolve(A source, C destination, string destMember, ResolutionContext context)\r\n        {\r\n            return \"from A->C\";\r\n        }\r\n\r\n        public string Resolve(B source, C destination, string destMember, ResolutionContext context)\r\n        {\r\n            return \"from B->C\";\r\n        }\r\n    }\r\n\r\n    class Program\r\n    {\r\n        static void Main(string[] args)\r\n        {\r\n            AutoMapper.Mapper.Initialize(x =>\r\n            {\r\n                x.CreateMap<B, C>()\r\n                    .ForMember(dest => dest.property1, opt => opt.ResolveUsing<MyTestResolver>());\r\n            });\r\n        }\r\n    }\r\n`","closed_by":{"login":"jbogard","id":104498,"node_id":"MDQ6VXNlcjEwNDQ5OA==","avatar_url":"https://avatars.githubusercontent.com/u/104498?v=4","gravatar_id":"","url":"https://api.github.com/users/jbogard","html_url":"https://github.com/jbogard","followers_url":"https://api.github.com/users/jbogard/followers","following_url":"https://api.github.com/users/jbogard/following{/other_user}","gists_url":"https://api.github.com/users/jbogard/gists{/gist_id}","starred_url":"https://api.github.com/users/jbogard/starred{/owner}{/repo}","subscriptions_url":"https://api.github.com/users/jbogard/subscriptions","organizations_url":"https://api.github.com/users/jbogard/orgs","repos_url":"https://api.github.com/users/jbogard/repos","events_url":"https://api.github.com/users/jbogard/events{/privacy}","received_events_url":"https://api.github.com/users/jbogard/received_events","type":"User","user_view_type":"public","site_admin":false},"reactions":{"url":"https://api.github.com/repos/LuckyPennySoftware/AutoMapper/issues/1887/r