	at org.apache.pdfbox.pdfparser.BaseParser.parseDirObject(BaseParser.java:1011)&lt;br/&gt;
	at org.apache.pdfbox.pdfparser.BaseParser.parseCOSDictionaryValue(BaseParser.java:179)&lt;br/&gt;
