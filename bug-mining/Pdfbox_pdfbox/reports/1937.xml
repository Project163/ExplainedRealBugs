&#160;&#160;&#160; at org.apache.pdfbox.text.PDFTextStripper.processPage(PDFTextStripper.java:391)
&#160;&#160;&#160; at org.apache.tika.parser.pdf.PDF2XHTML.processPage(PDF2XHTML.java:147)
