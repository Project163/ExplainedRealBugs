	at org.apache.pdfbox.pdfparser.BaseParser.parseDirObject(BaseParser.java:954) &lt;br/&gt;
	at org.apache.pdfbox.pdfparser.BaseParser.parseCOSArray(BaseParser.java:654) &lt;br/&gt;
	at org.apache.pdfbox.pdfparser.PDFStreamParser.parseNextToken(PDFStreamParser.java:175) &lt;br/&gt;
	at org.apache.pdfbox.contentstream.PDFStreamEngine.processStreamOperators(PDFStreamEngine.java:502) &lt;br/&gt;
	at org.apache.pdfbox.contentstream.PDFStreamEngine.processStream(PDFStreamEngine.java:469) &lt;br/&gt;
	at org.apache.pdfbox.contentstream.PDFStreamEngine.processPage(PDFStreamEngine.java:150) &lt;br/&gt;
	at org.apache.pdfbox.text.LegacyPDFStreamEngine.processPage(LegacyPDFStreamEngine.java:139) &lt;br/&gt;
