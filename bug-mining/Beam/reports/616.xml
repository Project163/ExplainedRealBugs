            &lt;span class=&quot;code-quote&quot;&gt;&quot;type&quot;&lt;/span&gt;: &lt;span class=&quot;code-quote&quot;&gt;&quot;STRING&quot;&lt;/span&gt;, 
            &lt;span class=&quot;code-quote&quot;&gt;&quot;value&quot;&lt;/span&gt;: &lt;span class=&quot;code-quote&quot;&gt;&quot;apache_beam.runners.dataflow.native_io.streaming_create.DecodeAndEmitDoFn&quot;&lt;/span&gt;
