at is causing the error, or so it appears.","author_association":"NONE","reactions":{"url":"https://api.github.com/repos/hashicorp/vagrant/issues/comments/388570009/reactions","total_count":0,"+1":0,"-1":0,"laugh":0,"hooray":0,"confused":0,"heart":0,"rocket":0,"eyes":0},"performed_via_github_app":null,"event":"commented","actor":{"login":"carestad","id":65263,"node_id":"MDQ6VXNlcjY1MjYz","avatar_url":"https://avatars.githubusercontent.com/u/65263?v=4","gravatar_id":"","url":"https://api.github.com/users/carestad","html_url":"https://github.com/carestad","followers_url":"https://api.github.com/users/carestad/followers","following_url":"https://api.github.com/users/carestad/following{/other_user}","gists_url":"https://api.github.com/users/carestad/gists{/gist_id}","starred_url":"https://api.github.com/users/carestad/starred{/owner}{/repo}","subscriptions_url":"https://api.github.com/users/carestad/subscriptions","organizations_url":"https://api.github.com/users/carestad/orgs","repos_url":"https://api.github.com/users/carestad/repos","events_url":"https://api.github.com/users/carestad/events{/privacy}","received_events_url":"https://api.github.com/users/carestad/received_events","type":"User","user_view_type":"public","site_admin":false}},{"url":"https://api.github.com/repos/hashicorp/vagrant/issues/comments/388574009","html_url":"https://github.com/hashicorp/vagrant/issues/9806#issuecomment-388574009","issue_url":"https://api.github.com/repos/hashicorp/vagrant/issues/9806","id":388574009,"node_id":"MDEyOklzc3VlQ29tbWVudDM4ODU3NDAwOQ==","user":{"login":"carestad","id":65263,"node_id":"MDQ6VXNlcjY1MjYz","avatar_url":"https://avatars.githubusercontent.com/u/65263?v=4","gravatar_id":"","url":"https://api.github.com/users/carestad","html_url":"https://github.com/carestad","followers_url":"https://api.github.com/users/carestad/followers","following_url":"https://api.github.com/users/carestad/following{/other_user}","gists_url":"https://api.github.com/users/carestad/gists{/g