d]);\r\n        }\r\n        \r\n        $finders = array_change_key_case($instance->implementedFinders());\r\n        foreach ($finders as $finder=>$finderName) {\r\n            unset($this->_finderMap[$finder]);\r\n        }\r\n\r\n        return $result;\r\n    }\r\n```","author_association":"NONE","reactions":{"url":"https://api.github.com/repos/cakephp/cakephp/issues/comments/2315101770/reactions","total_count":0,"+1":0,"-1":0,"laugh":0,"hooray":0,"confused":0,"heart":0,"rocket":0,"eyes":0},"performed_via_github_app":null,"event":"commented","actor":{"login":"kuebel","id":9949019,"node_id":"MDQ6VXNlcjk5NDkwMTk=","avatar_url":"https://avatars.githubusercontent.com/u/9949019?v=4","gravatar_id":"","url":"https://api.github.com/users/kuebel","html_url":"https://github.com/kuebel","followers_url":"https://api.github.com/users/kuebel/followers","following_url":"https://api.github.com/users/kuebel/following{/other_user}","gists_url":"https://api.github.com/users/kuebel/gists{/gist_id}","starred_url":"https://api.github.com/users/kuebel/starred{/owner}{/repo}","subscriptions_url":"https://api.github.com/users/kuebel/subscriptions","organizations_url":"https://api.github.com/users/kuebel/orgs","repos_url":"https://api.github.com/users/kuebel/repos","events_url":"https://api.github.com/users/kuebel/events{/privacy}","received_events_url":"https://api.github.com/users/kuebel/received_events","type":"User","user_view_type":"public","site_admin":false}},{"id":14055104671,"node_id":"REFE_lADOAAoEbs6LfrBwzwAAAANFv-Cf","url":"https://api.github.com/repos/cakephp/cakephp/issues/events/14055104671","actor":{"login":"markstory","id":24086,"node_id":"MDQ6VXNlcjI0MDg2","avatar_url":"https://avatars.githubusercontent.com/u/24086?v=4","gravatar_id":"","url":"https://api.github.com/users/markstory","html_url":"https://github.com/markstory","followers_url":"https://api.github.com/users/markstory/followers","following_url":"https://api.github.com/users/markstory/following{/other_user}",