 be wrong though, i haven't tested this)","author_association":"CONTRIBUTOR","reactions":{"url":"https://api.github.com/repos/sveltejs/kit/issues/comments/1446168314/reactions","total_count":0,"+1":0,"-1":0,"laugh":0,"hooray":0,"confused":0,"heart":0,"rocket":0,"eyes":0},"performed_via_github_app":null,"event":"commented","actor":{"login":"bfanger","id":207248,"node_id":"MDQ6VXNlcjIwNzI0OA==","avatar_url":"https://avatars.githubusercontent.com/u/207248?v=4","gravatar_id":"","url":"https://api.github.com/users/bfanger","html_url":"https://github.com/bfanger","followers_url":"https://api.github.com/users/bfanger/followers","following_url":"https://api.github.com/users/bfanger/following{/other_user}","gists_url":"https://api.github.com/users/bfanger/gists{/gist_id}","starred_url":"https://api.github.com/users/bfanger/starred{/owner}{/repo}","subscriptions_url":"https://api.github.com/users/bfanger/subscriptions","organizations_url":"https://api.github.com/users/bfanger/orgs","repos_url":"https://api.github.com/users/bfanger/repos","events_url":"https://api.github.com/users/bfanger/events{/privacy}","received_events_url":"https://api.github.com/users/bfanger/received_events","type":"User","user_view_type":"public","site_admin":false}},{"url":"https://api.github.com/repos/sveltejs/kit/issues/comments/1446464208","html_url":"https://github.com/sveltejs/kit/issues/9225#issuecomment-1446464208","issue_url":"https://api.github.com/repos/sveltejs/kit/issues/9225","id":1446464208,"node_id":"IC_kwDOEiPr8c5WN0rQ","user":{"login":"Rich-Harris","id":1162160,"node_id":"MDQ6VXNlcjExNjIxNjA=","avatar_url":"https://avatars.githubusercontent.com/u/1162160?v=4","gravatar_id":"","url":"https://api.github.com/users/Rich-Harris","html_url":"https://github.com/Rich-Harris","followers_url":"https://api.github.com/users/Rich-Harris/followers","following_url":"https://api.github.com/users/Rich-Harris/following{/other_user}","gists_url":"https://api.github.com/users/Rich-Harris/gists{/gist_id}"