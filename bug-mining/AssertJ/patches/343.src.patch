+  void should_throw_error_if_actual_range_is_null() {
+    assertThatExceptionOfType(AssertionError.class).isThrownBy(() -> rangeSets.assertEnclosesAll(someInfo(), null,
+                                                                                                 iterable(closed(1, 2))))
