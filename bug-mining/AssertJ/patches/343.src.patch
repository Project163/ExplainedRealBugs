+package org.assertj.guava.internal.rangesets;
+
+import static org.assertj.core.api.Assertions.assertThatExceptionOfType;
+import static org.assertj.core.error.ShouldBeNullOrEmpty.shouldBeNullOrEmpty;
+
+import org.assertj.guava.internal.RangeSetsBaseTest;
+import org.junit.jupiter.api.Test;
+
+class RangeSets_assertNullOrEmpty_Test extends RangeSetsBaseTest {
+
+  @Test
+  void should_pass_if_actual_is_empty() {
+    actual.clear();
+    rangeSets.assertNullOrEmpty(someInfo(), actual);
+  }
+
+  @Test
+  void should_pass_if_actual_is_null() {
+    actual.clear();
+    rangeSets.assertNullOrEmpty(someInfo(), null);
+  }
+
+  @Test
+  void should_fail_if_actual_is_not_empty() {
+    assertThatExceptionOfType(AssertionError.class).isThrownBy(() -> rangeSets.assertNullOrEmpty(someInfo(), actual))
+                                                   .withMessage(shouldBeNullOrEmpty(actual).create());
+  }
