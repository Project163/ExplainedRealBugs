+  void should_pass_if_actual_size_matches_expected() {
+    // GIVEN
+    RangeSet<Integer> actual = ImmutableRangeSet.<Integer> builder()
+                                                .add(closed(1, 10))
+                                                .add(closed(20, 35))
+                                                .add(closed(40, 45))
+                                                .build();
+    // THEN
+    assertThat(actual).hasSize(actual.asRanges().size());
+  }
