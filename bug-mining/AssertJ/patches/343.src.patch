+    RangeSet<Integer> actual = of(closed(1, 10));
+    // THEN
+    assertThat(actual).intersectsAnyRangesOf(asList(open(0, 1),
+                                                      open(1, 4),
+                                                      closed(3, 11),
+                                                      closed(11, 15)));
+  }
+
