               String fileName = StringUtils.replace( event.getFileName(), &quot;&lt;br class=&quot;atl-forced-newline&quot; /&gt;&quot;, &quot;/&quot; );&lt;br/&gt;
               if ( fileName.startsWith( sourceDirectoryPath + &quot;/&quot; ) )&lt;/p&gt;
               {
                   currentFile = StringUtils.substring( fileName, sourceDirectoryPath.length() + 1 );
   
                   events = getResults().getFileViolations( currentFile );
               }
&lt;p&gt;           }&lt;/p&gt;

&lt;p&gt;           if ( events == null )&lt;/p&gt;
           {
               events = new ArrayList&amp;lt;AuditEvent&amp;gt;();
