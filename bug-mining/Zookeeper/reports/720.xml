[zk: localhost:2181(CONNECTED) 8] get /contacts
{&lt;span class=&quot;code-quote&quot;&gt;&quot;country&quot;&lt;/span&gt;:&lt;span class=&quot;code-quote&quot;&gt;&quot;CA&quot;&lt;/span&gt;,&lt;span class=&quot;code-quote&quot;&gt;&quot;name&quot;&lt;/span&gt;:&quot;De
