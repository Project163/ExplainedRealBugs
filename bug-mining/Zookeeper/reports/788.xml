        &lt;span class=&quot;code-object&quot;&gt;String&lt;/span&gt;[] args = &lt;span class=&quot;code-keyword&quot;&gt;new&lt;/span&gt; &lt;span class=&quot;code-object&quot;&gt;String&lt;/span&gt;[] { &lt;span class=&quot;code-quote&quot;&gt;&quot;123&quot;&lt;/span&gt;, &lt;span class=&quot;code-quote&quot;&gt;&quot;dataDir&quot;&lt;/span&gt;, &lt;span class=&quot;code-object&quot;&gt;Long&lt;/span&gt;.toString(tickTime), &lt;span class=&quot;code-quote&quot;&gt;&quot;60&quot;&lt;/span&gt; };
