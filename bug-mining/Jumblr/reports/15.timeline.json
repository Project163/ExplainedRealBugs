ests added due to refactoring that would be necessary to add them.","reactions":{"url":"https://api.github.com/repos/tumblr/jumblr/issues/116/reactions","total_count":0,"+1":0,"-1":0,"laugh":0,"hooray":0,"confused":0,"heart":0,"rocket":0,"eyes":0},"timeline_url":"https://api.github.com/repos/tumblr/jumblr/issues/116/timeline","performed_via_github_app":null,"state_reason":null}},"event":"cross-referenced"},{"id":1303736082,"node_id":"MDExOkNsb3NlZEV2ZW50MTMwMzczNjA4Mg==","url":"https://api.github.com/repos/tumblr/jumblr/issues/events/1303736082","actor":{"login":"ceyko","id":172422,"node_id":"MDQ6VXNlcjE3MjQyMg==","avatar_url":"https://avatars.githubusercontent.com/u/172422?v=4","gravatar_id":"","url":"https://api.github.com/users/ceyko","html_url":"https://github.com/ceyko","followers_url":"https://api.github.com/users/ceyko/followers","following_url":"https://api.github.com/users/ceyko/following{/other_user}","gists_url":"https://api.github.com/users/ceyko/gists{/gist_id}","starred_url":"https://api.github.com/users/ceyko/starred{/owner}{/repo}","subscriptions_url":"https://api.github.com/users/ceyko/subscriptions","organizations_url":"https://api.github.com/users/ceyko/orgs","repos_url":"https://api.github.com/users/ceyko/repos","events_url":"https://api.github.com/users/ceyko/events{/privacy}","received_events_url":"https://api.github.com/users/ceyko/received_events","type":"User","user_view_type":"public","site_admin":false},"event":"closed","commit_id":null,"commit_url":null,"created_at":"2017-10-20T20:41:11Z","state_reason":null,"performed_via_github_app":null}]