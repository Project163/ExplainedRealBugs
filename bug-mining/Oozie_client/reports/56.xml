@NamedQuery(name = &quot;GET_ACTIONS_FOR_COORD_JOB_ORDER_BY_NOMINAL_TIME&quot;, query = &quot;select a.id, a.actionNumber, a.consoleUrl, a.errorCode, a.errorMessage, a.externalId, a.externalStatus, a.jobId, a.trackerUri, a.createdTimestamp, a.nominalTimestamp, a.status, a.lastModifiedTimestamp, a.missingDependencies, a.timeOut from CoordinatorActionBean a where a.jobId = :jobId order by a.nominalTimestamp desc&quot;)&lt;/p&gt;</comment>
