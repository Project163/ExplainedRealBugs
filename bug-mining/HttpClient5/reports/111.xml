&lt;p&gt;&#160; &lt;font color=&quot;#9e880d&quot;&gt;@Test&lt;/font&gt;&lt;font color=&quot;#9e880d&quot;&gt;&#160; &lt;/font&gt;&lt;font color=&quot;#0033b3&quot;&gt;fun &lt;/font&gt;&lt;font color=&quot;#00627a&quot;&gt;bugbug&lt;/font&gt;() { &lt;br/&gt;
&#160;&#160; &lt;font color=&quot;#0033b3&quot;&gt;val &lt;/font&gt;&lt;font color=&quot;#000000&quot;&gt;client &lt;/font&gt;= &lt;font color=&quot;#000000&quot;&gt;HttpClients&lt;/font&gt;.custom()&lt;br/&gt;
&#160;&#160;&#160; .setConnectionManager(BasicHttpClientConnectionManager())&lt;br/&gt;
&lt;font color=&quot;#8c8c8c&quot;&gt;// .setConnectionManager(PoolingHttpClientConnectionManager())&lt;/font&gt;&#160;&#160; .setDefaultRequestConfig(&lt;br/&gt;
&#160;&#160;&#160; &lt;font color=&quot;#000000&quot;&gt;RequestConfig&lt;/font&gt;.custom()&lt;br/&gt;
&#160;&#160;&#160;&#160; .setResponseTimeout(&lt;font color=&quot;#000000&quot;&gt;Timeout&lt;/font&gt;.ofSeconds(&lt;font color=&quot;#1750eb&quot;&gt;1&lt;/font&gt;))&lt;br/&gt;
&#160;&#160;&#160;&#160; .build()&lt;br/&gt;
&#160;&#160;&#160; )&lt;br/&gt;
&#160;&#160; .build()&lt;/p&gt;

&lt;p&gt;&#160;&#160; &lt;font color=&quot;#000000&quot;&gt;client&lt;/font&gt;.&lt;font color=&quot;#00627a&quot;&gt;executeAndLog&lt;/font&gt;(&lt;font color=&quot;#067d17&quot;&gt;&quot;https://httpbin.org/anything?1&quot;&lt;/font&gt;)&lt;br/&gt;
&#160;&#160; &lt;font color=&quot;#000000&quot;&gt;client&lt;/font&gt;.&lt;font color=&quot;#00627a&quot;&gt;executeAndLog&lt;/font&gt;(&lt;font color=&quot;#067d17&quot;&gt;&quot;https://httpbin.org/delay/2?2&quot;&lt;/font&gt;)&lt;br/&gt;
&#160;&#160; &lt;font color=&quot;#000000&quot;&gt;client&lt;/font&gt;.&lt;font color=&quot;#00627a&quot;&gt;executeAndLog&lt;/font&gt;(&lt;font color=&quot;#067d17&quot;&gt;&quot;https://httpbin.org/delay/2?2&quot;&lt;/font&gt;)&lt;br/&gt;
&#160;&#160; &lt;font color=&quot;#000000&quot;&gt;client&lt;/font&gt;.&lt;font color=&quot;#00627a&quot;&gt;executeAndLog&lt;/font&gt;(&lt;font color=&quot;#067d17&quot;&gt;&quot;https://httpbin.org/anything?3&quot;&lt;/font&gt;)&lt;br/&gt;
&#160;&#160; &lt;font color=&quot;#000000&quot;&gt;client&lt;/font&gt;.&lt;font color=&quot;#00627a&quot;&gt;executeAndLog&lt;/font&gt;(&lt;font color=&quot;#067d17&quot;&gt;&quot;https://httpbin.org/anything?3&quot;&lt;/font&gt;)&lt;br/&gt;
&#160;&#160; &lt;font color=&quot;#000000&quot;&gt;client&lt;/font&gt;.&lt;font color=&quot;#00627a&quot;&gt;executeAndLog&lt;/font&gt;(&lt;font color=&quot;#067d17&quot;&gt;&quot;https://httpbin.org/anything?3&quot;&lt;/font&gt;)&lt;br/&gt;
}&lt;/p&gt;

