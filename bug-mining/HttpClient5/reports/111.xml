&#160; } &lt;font color=&quot;#0033b3&quot;&gt;catch &lt;/font&gt;(&lt;font color=&quot;#000000&quot;&gt;ex&lt;/font&gt;: &lt;font color=&quot;#000000&quot;&gt;Exception&lt;/font&gt;) {&lt;br/&gt;
&#160; &lt;font color=&quot;#00627a&quot;&gt;println&lt;/font&gt;(&lt;font color=&quot;#067d17&quot;&gt;&quot;&lt;/font&gt;&lt;font color=&quot;#0037a6&quot;&gt;${color}uri&lt;/font&gt;&lt;font color=&quot;#067d17&quot;&gt; -&amp;gt; &lt;/font&gt;&lt;font color=&quot;#0037a6&quot;&gt;${color}ex&lt;/font&gt;&lt;font color=&quot;#067d17&quot;&gt;&quot;&lt;/font&gt;)&lt;br/&gt;
&#160; }&lt;br/&gt;
