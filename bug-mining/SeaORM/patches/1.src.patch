+
+impl DbScheme {
+    pub fn starts_with(self, base_url: &str) -> bool {
+        match self {
+            DbScheme::Postgres => {
+                base_url.starts_with("postgres://") || base_url.starts_with("postgresql://")
+            }
+            DbScheme::Mysql => base_url.starts_with("mysql://"),
+            DbScheme::Sqlite => base_url.starts_with("sqlite:"),
+        }
+    }
+}
