+    opens tools.jackson.dataformat.cbor.parse;
+    opens tools.jackson.dataformat.cbor.seq;
+    opens tools.jackson.dataformat.cbor.testutil;
+    opens tools.jackson.dataformat.cbor.testutil.failure;
