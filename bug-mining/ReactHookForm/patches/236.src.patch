+      <ul>
+        {fields.map((item, i) => (
+          <Controller
+            key={item.id}
+            render={({ field }) => <input {...field} aria-label={'name'} />}
+            name={`test.${index}.keyValue.${i}.name`}
+            control={control}
+            defaultValue={item.name}
+          />
