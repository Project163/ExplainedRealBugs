+
+const SubForm = ({ control }: { control: Control<FormInputs> }) => {
+  const {
+    isDirty,
+    dirtyFields,
+    touchedFields,
+    isSubmitted,
+    isSubmitSuccessful,
+    submitCount,
+    isValid,
+  } = useFormState({
