            [currentError.path[0]]: currentError\n          };\n        }, {})\n      : {}\n  };\n};\n\nexport default function App() {\n  const { register, handleSubmit, errors } = useForm({\n    validationResolver: resolver,\n    validationContext: { test: \"test\" }\n  });\n\n  return (\n    &lt;form onSubmit={handleSubmit(d =&gt; console.log(d))}&gt;\n      &lt;input type=\"text\" name=\"username\" ref={register} /&gt;\n      &lt;input type=\"submit\" /&gt;\n    &lt;/form&gt;\n  );\n}\n</code></pre>\n<p><g-emoji class=\"g-emoji\" alias=\"beetle\" fallback-src=\"https://github.githubassets.com/images/icons/emoji/unicode/1f41e.png\">üêû</g-emoji> close <a class=\"issue-link js-issue-link\" data-error-text=\"Failed to load title\" data-id=\"558764718\" data-permission-text=\"Title is private\" data-url=\"https://github.com/react-hook-form/react-hook-form/issues/954\" data-hovercard-type=\"issue\" data-hovercard-url=\"/react-hook-form/react-hook-form/issues/954/hovercard\" href=\"https://github.com/react-hook-form/react-hook-form/issues/954\">#954</a> fix type for ErrorMessage component props (<a class=\"issue-link js-issue-link\" data-error-text=\"Failed to load title\" data-id=\"559455753\" data-permission-text=\"Title is private\" data-url=\"https://github.com/react-hook-form/react-hook-form/issues/966\" data-hovercard-type=\"pull_request\" data-hovercard-url=\"/react-hook-form/react-hook-form/pull/966/hovercard\" href=\"https://github.com/react-hook-form/react-hook-form/pull/966\">#966</a>)<br>\n<g-emoji class=\"g-emoji\" alias=\"beetle\" fallback-src=\"https://github.githubassets.com/images/icons/emoji/unicode/1f41e.png\">üêû</g-emoji> fix <a class=\"issue-link js-issue-link\" data-error-text=\"Failed to load title\" data-id=\"559459941\" data-permission-text=\"Title is private\" data-url=\"https://github.com/react-hook-form/react-hook-form/issues/967\" data-hovercard-type=\"issue\" data-hovercard-url=\"/react-hook-form/react-hook-form/issues/967/hovercard\" 