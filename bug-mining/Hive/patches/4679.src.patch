+      StructColumnVector struct = (StructColumnVector) vector;
+      for (int f = 0; f < childrenTypes.size(); ++f) {
+        setStructCol(result, f, nextValue(struct.fields[f], row,
+            childrenTypes.get(f), getStructCol(result, f)));
+      }
