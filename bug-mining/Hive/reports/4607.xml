 org.apache.hadoop.hive.ql.lockmgr.TestDbTxnManager2.testLocksInSubquery	5.9 sec	1&lt;br/&gt;
 org.apache.hive.minikdc.TestJdbcWithDBTokenStore.testNegativeTokenAuth	0.76 sec	1&lt;br/&gt;
 org.apache.hadoop.hive.llap.tezplugins.TestLlapTaskCommunicator.testFinishableStateUpdateFailure	5 sec	1&lt;br/&gt;
 org.apache.hadoop.hive.cli.TestMiniTezCliDriver.testCliDriver_bucket3	12 sec	1&lt;br/&gt;
 org.apache.hive.jdbc.TestSSL.testSSLConnectionWithURL	33 sec	1&lt;br/&gt;
 org.apache.hive.jdbc.TestSSL.testSSLConnectionWithProperty	33 sec	1&lt;br/&gt;
 org.apache.hive.jdbc.TestSSL.testConnectionWrongCertCN	45 sec	1&lt;br/&gt;
 org.apache.hadoop.hive.llap.daemon.impl.comparator.TestShortestJobFirstComparator.testWaitQueueComparatorWithinDagPriority	5 sec	1&lt;/p&gt;</comment>
                            <comment id="15310728" author="wzheng" created="Wed, 1 Jun 2016 17:37:29 +0000"  >&lt;p&gt;Committed to master. Thanks Eugene for the review.&lt;/p&gt;</comment>
                            <comment id="15311533" author="ekoifman" created="Thu, 2 Jun 2016 01:08:46 +0000"  >&lt;p&gt;&lt;a href=&quot;https://issues.apache.org/jira/secure/ViewProfile.jspa?name=wzheng&quot; class=&quot;user-hover&quot; rel=&quot;wzheng&quot;&gt;wzheng&lt;/a&gt;&lt;br/&gt;
org.apache.hadoop.hive.ql.lockmgr.TestDbTxnManager2.testLocksInSubquery	5.9 sec	1&lt;br/&gt;
