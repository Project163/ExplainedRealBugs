    &lt;span class=&quot;code-keyword&quot;&gt;return&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;new&lt;/span&gt; EncodedReaderImpl(fileId, types,
        codec, bufferSize, rowIndexStride, dataCache, dataReader, pf);
  }
