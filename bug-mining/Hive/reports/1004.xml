&lt;pre class=&quot;code-java&quot;&gt;{
    &lt;span class=&quot;code-quote&quot;&gt;&quot;qfile_hosts&quot;&lt;/span&gt;: [
        [&lt;span class=&quot;code-quote&quot;&gt;&quot;hostname1&quot;&lt;/span&gt;, 2],
