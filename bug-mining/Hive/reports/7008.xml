<item>
            <title>[HIVE-21104] PTF with nested structure throws ClassCastException</title>
                <link>https://issues.apache.org/jira/browse/HIVE-21104</link>
                <project id="12310843" key="HIVE">Hive</project>
                    <description>
&lt;div class=&quot;preformatted panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;preformattedContent panelContent&quot;&gt;
&lt;pre&gt;DROP TABLE IF EXISTS dummy;
CREATE TABLE dummy (i int);
INSERT INTO TABLE dummy VALUES (1);

DROP TABLE IF EXISTS struct_table_example;
CREATE TABLE struct_table_example (a int, s1 struct&amp;lt;f1: boolean, f2: string, f3: int, f4: int&amp;gt; ) STORED AS ORC;

INSERT INTO TABLE struct_table_example SELECT 1, named_struct(&apos;f1&apos;, false, &apos;f2&apos;, &apos;test&apos;, &apos;f3&apos;, 3, &apos;f4&apos;, 4)  FROM dummy;

select s1.f1, s1.f2, rank() over (partition by s1.f2 order by s1.f4) from struct_table_example;
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;

&lt;p&gt;This would throw the following error&lt;/p&gt;

