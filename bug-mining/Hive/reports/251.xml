                    <description>
&lt;p&gt;set hive.mapred.mode=strict;&lt;/p&gt;

&lt;p&gt;select * from &lt;br/&gt;
  (select count(1) from src &lt;br/&gt;
    union all&lt;br/&gt;
   select count(1) from srcpart where ds = &apos;2009-08-09&apos;&lt;br/&gt;
