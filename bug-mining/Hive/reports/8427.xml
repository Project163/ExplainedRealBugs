            <title>[HIVE-26922] Deadlock when rebuilding Materialized view stored by Iceberg</title>
                <link>https://issues.apache.org/jira/browse/HIVE-26922</link>
                <project id="12310843" key="HIVE">Hive</project>
                    <description>&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt;
create table tbl_ice(a &lt;span class=&quot;code-object&quot;&gt;int&lt;/span&gt;, b string, c &lt;span class=&quot;code-object&quot;&gt;int&lt;/span&gt;) stored by iceberg stored as orc tblproperties (&lt;span class=&quot;code-quote&quot;&gt;&apos;format-version&apos;&lt;/span&gt;=&lt;span class=&quot;code-quote&quot;&gt;&apos;1&apos;&lt;/span&gt;);
insert into tbl_ice values (1, &lt;span class=&quot;code-quote&quot;&gt;&apos;one&apos;&lt;/span&gt;, 50), (2, &lt;span class=&quot;code-quote&quot;&gt;&apos;two&apos;&lt;/span&gt;, 51), (3, &lt;span class=&quot;code-quote&quot;&gt;&apos;three&apos;&lt;/span&gt;, 52), (4, &lt;span class=&quot;code-quote&quot;&gt;&apos;four&apos;&lt;/span&gt;, 53), (5, &lt;span class=&quot;code-quote&quot;&gt;&apos;five&apos;&lt;/span&gt;, 54);

create materialized view mat1 stored by iceberg stored as orc tblproperties (&lt;span class=&quot;code-quote&quot;&gt;&apos;format-version&apos;&lt;/span&gt;=&lt;span class=&quot;code-quote&quot;&gt;&apos;1&apos;&lt;/span&gt;) as
select tbl_ice.b, tbl_ice.c from tbl_ice where tbl_ice.c &amp;gt; 52;

insert into tbl_ice values (10, &lt;span class=&quot;code-quote&quot;&gt;&apos;ten&apos;&lt;/span&gt;, 60);

alter materialized view mat1 rebuild;
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;</description>
