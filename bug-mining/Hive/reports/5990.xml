<item>
            <title>[HIVE-18157] Vectorization : Insert in bucketed table is broken with vectorization</title>
                <link>https://issues.apache.org/jira/browse/HIVE-18157</link>
                <project id="12310843" key="HIVE">Hive</project>
                    <description>&lt;p&gt;create temporary table foo (x int) clustered by &lt;img class=&quot;emoticon&quot; src=&quot;https://issues.apache.org/jira/images/icons/emoticons/error.png&quot; height=&quot;16&quot; width=&quot;16&quot; align=&quot;absmiddle&quot; alt=&quot;&quot; border=&quot;0&quot;/&gt; into 4 buckets;&lt;br/&gt;
insert overwrite table foo values(1),(2),(3),(4),(9);&lt;/p&gt;

&lt;p&gt;select &lt;b&gt;, regexp_extract(INPUT_&lt;em&gt;FILE&lt;/em&gt;_NAME, &apos;.&lt;/b&gt;/(.*)&apos;, 1) from foo;&lt;br/&gt;
OK&lt;br/&gt;
9       000000_0&lt;br/&gt;
4       000000_0&lt;br/&gt;
3       000000_0&lt;br/&gt;
2       000000_0&lt;br/&gt;
1       000000_0&lt;/p&gt;

&lt;p&gt;set hive.vectorized.execution.enabled=false;&lt;br/&gt;
insert overwrite table foo values(1),(2),(3),(4),(9);&lt;br/&gt;
select &lt;b&gt;, regexp_extract(INPUT_&lt;em&gt;FILE&lt;/em&gt;_NAME, &apos;.&lt;/b&gt;/(.*)&apos;, 1) from foo;&lt;br/&gt;
OK&lt;br/&gt;
4       000000_0&lt;br/&gt;
9       000001_0&lt;br/&gt;
1       000001_0&lt;br/&gt;
2       000002_0&lt;br/&gt;
3       000003_0&lt;/p&gt;</description>
