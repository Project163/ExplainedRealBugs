        (
          (
            ((key &amp;lt;&amp;gt; &lt;span class=&quot;code-quote&quot;&gt;&apos;302&apos;&lt;/span&gt;) and (key &amp;lt;&amp;gt; &lt;span class=&quot;code-quote&quot;&gt;&apos;311&apos;&lt;/span&gt;)) - 500 rows
            and
            ((value &amp;lt;&amp;gt; &lt;span class=&quot;code-quote&quot;&gt;&apos;val_50&apos;&lt;/span&gt;) or (key &amp;gt; &lt;span class=&quot;code-quote&quot;&gt;&apos;1&apos;&lt;/span&gt;)) - 666 rows (This is wrong. &lt;span class=&quot;code-keyword&quot;&gt;this&lt;/span&gt; should never exceed max number of rows)
          )
          and
          (key &amp;lt; &lt;span class=&quot;code-quote&quot;&gt;&apos;400&apos;&lt;/span&gt;) - 222 rows
        )
        and (key &amp;lt;&amp;gt; &lt;span class=&quot;code-quote&quot;&gt;&apos;305&apos;&lt;/span&gt;) - 222 rows
      )
      and (key &amp;lt;&amp;gt; &lt;span class=&quot;code-quote&quot;&gt;&apos;14&apos;&lt;/span&gt;) - 222 rows
