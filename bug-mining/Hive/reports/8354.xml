<item>
            <title>[HIVE-25848] Empty result for structs in point lookup optimization with vectorization on</title>
                <link>https://issues.apache.org/jira/browse/HIVE-25848</link>
                <project id="12310843" key="HIVE">Hive</project>
                    <description>&lt;p&gt;Repro steps:&lt;/p&gt;
&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt;
set hive.fetch.task.conversion=none;

create table test (a string) partitioned by (y string, m string);
insert into test values (&lt;span class=&quot;code-quote&quot;&gt;&apos;aa&apos;&lt;/span&gt;, 2022, 1);

select * from test where (y=year(date_sub(current_date,4)) and m=month(date_sub(current_date,4))) or (y=year(date_sub(current_date,10)) and m=month(date_sub(current_date,10)) );

--gives empty result&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;
&lt;p&gt;Turning either of the feature below off yields to good result (1 row expected):&lt;/p&gt;
&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt;
set hive.optimize.point.lookup=&lt;span class=&quot;code-keyword&quot;&gt;false&lt;/span&gt;;
set hive.cbo.enable=&lt;span class=&quot;code-keyword&quot;&gt;false&lt;/span&gt;;
set hive.vectorized.execution.enabled=&lt;span class=&quot;code-keyword&quot;&gt;false&lt;/span&gt;;
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;
&lt;p&gt;Expected good result is:&lt;/p&gt;
&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt;
+---------+---------+---------+
| test.a &#160;| test.y &#160;| test.m &#160;|
+---------+---------+---------+
| aa &#160; &#160; &#160;| 2022 &#160; &#160;| 1 &#160; &#160; &#160; |
+---------+---------+---------+ &lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;</description>
                <environment></environment>
