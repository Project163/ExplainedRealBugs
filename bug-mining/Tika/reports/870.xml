                            <comment id="15818551" author="hafkensite" created="Wed, 11 Jan 2017 14:57:29 +0000"  >&lt;p&gt;Unit test that causes the exception to occur.&lt;/p&gt;
&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt;@Test
&lt;span class=&quot;code-keyword&quot;&gt;public&lt;/span&gt; void tikaTest() &lt;span class=&quot;code-keyword&quot;&gt;throws&lt;/span&gt; IOException {
    Metadata metadata = &lt;span class=&quot;code-keyword&quot;&gt;new&lt;/span&gt; Metadata();
    metadata.add(Metadata.CONTENT_TYPE, MediaType.text(&lt;span class=&quot;code-quote&quot;&gt;&quot;javascript&quot;&lt;/span&gt;).toString());
    InputStream input = &lt;span class=&quot;code-keyword&quot;&gt;new&lt;/span&gt; ByteArrayInputStream((&lt;span class=&quot;code-quote&quot;&gt;&quot;function() {};\n&quot;&lt;/span&gt; +
            &lt;span class=&quot;code-quote&quot;&gt;&quot;&lt;span class=&quot;code-keyword&quot;&gt;try&lt;/span&gt; {\n&quot;&lt;/span&gt; +
            &lt;span class=&quot;code-quote&quot;&gt;&quot;    window.location = &lt;span class=&quot;code-quote&quot;&gt;&apos;index.html&apos;&lt;/span&gt;;\n&quot;&lt;/span&gt; +
            &lt;span class=&quot;code-quote&quot;&gt;&quot;} &lt;span class=&quot;code-keyword&quot;&gt;catch&lt;/span&gt; (e) {\n&quot;&lt;/span&gt; +
            &lt;span class=&quot;code-quote&quot;&gt;&quot;    console.log(e);\n&quot;&lt;/span&gt; +
            &lt;span class=&quot;code-quote&quot;&gt;&quot;}&quot;&lt;/span&gt;).getBytes(StandardCharsets.UTF_8));
    MediaType detect = &lt;span class=&quot;code-keyword&quot;&gt;new&lt;/span&gt; ProbabilisticMimeDetectionSelector().detect(input, metadata);
    assertEquals(MediaType.text(&lt;span class=&quot;code-quote&quot;&gt;&quot;javascript&quot;&lt;/span&gt;), detect);
}
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;</comment>
