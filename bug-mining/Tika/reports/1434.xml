&amp;lt;meta name=&lt;span class=&quot;code-quote&quot;&gt;&quot;X-TIKA:Parsed-By&quot;&lt;/span&gt; content=&lt;span class=&quot;code-quote&quot;&gt;&quot;org.apache.tika.parser.DefaultParser&quot;&lt;/span&gt;/&amp;gt;
&amp;lt;meta name=&lt;span class=&quot;code-quote&quot;&gt;&quot;X-TIKA:Parsed-By&quot;&lt;/span&gt; content=&lt;span class=&quot;code-quote&quot;&gt;&quot;org.apache.tika.parser.pkg.PackageParser&quot;&lt;/span&gt;/&amp;gt;
&amp;lt;meta name=&lt;span class=&quot;code-quote&quot;&gt;&quot;resourceName&quot;&lt;/span&gt; content=&lt;span class=&quot;code-quote&quot;&gt;&quot;testZipEntryNameCharsetShiftSJIS.zip&quot;&lt;/span&gt;/&amp;gt;
&amp;lt;meta name=&lt;span class=&quot;code-quote&quot;&gt;&quot;X-TIKA:detectedEncoding&quot;&lt;/span&gt; content=&lt;span class=&quot;code-quote&quot;&gt;&quot;ISO-8859-1&quot;&lt;/span&gt;/&amp;gt;
&amp;lt;meta name=&lt;span class=&quot;code-quote&quot;&gt;&quot;Content-Length&quot;&lt;/span&gt; content=&lt;span class=&quot;code-quote&quot;&gt;&quot;28885&quot;&lt;/span&gt;/&amp;gt;
