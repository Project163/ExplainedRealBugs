                            <comment id="13681596" author="frankgrimes97" created="Wed, 12 Jun 2013 20:29:48 +0000"  >&lt;p&gt;diff --git a/project/Build.scala b/project/Build.scala&lt;br/&gt;
index f177215..ecc74d4 100644&lt;br/&gt;
&amp;#8212; a/project/Build.scala&lt;br/&gt;
+++ b/project/Build.scala&lt;br/&gt;
@@ -34,7 +34,8 @@ object KafkaBuild extends Build {&lt;br/&gt;
     buildNumber := System.getProperty(&quot;build.number&quot;, &quot;&quot;),&lt;br/&gt;
     version &amp;lt;&amp;lt;= (buildNumber, version)  &lt;/p&gt;
{ (build, version)  =&amp;gt; if (build == &quot;&quot;) version else version + &quot;+&quot; + build}
&lt;p&gt;,&lt;br/&gt;
     releaseName &amp;lt;&amp;lt;= (name, version, scalaVersion) {(name, version, scalaVersion) =&amp;gt; name + &quot;_&quot; + scalaVersion + &quot;-&quot; + v&lt;/p&gt;
&lt;ul class=&quot;alternate&quot; type=&quot;square&quot;&gt;
	&lt;li&gt;javacOptions ++= Seq(&quot;-Xlint:unchecked&quot;, &quot;-source&quot;, &quot;1.5&quot;),&lt;br/&gt;
+    javacOptions in compile ++= Seq(&quot;-Xlint:unchecked&quot;, &quot;-source&quot;, &quot;1.5&quot;),&lt;br/&gt;
+    javacOptions in doc ++= Seq(&quot;-source&quot;, &quot;1.5&quot;),&lt;br/&gt;
     parallelExecution in Test := false, // Prevent tests from overrunning each other&lt;br/&gt;
     libraryDependencies ++= Seq(&lt;br/&gt;
       &quot;log4j&quot;                 % &quot;log4j&quot;        % &quot;1.2.15&quot;,&lt;br/&gt;
@@ -57,7 +58,7 @@ object KafkaBuild extends Build {&lt;br/&gt;
   )&lt;/li&gt;
&lt;/ul&gt;


&lt;p&gt;   val hadoopSettings = Seq(&lt;/p&gt;
&lt;ul class=&quot;alternate&quot; type=&quot;square&quot;&gt;
	&lt;li&gt;javacOptions ++= Seq(&quot;-Xlint:deprecation&quot;),&lt;br/&gt;
+    javacOptions in compile ++= Seq(&quot;-Xlint:deprecation&quot;),&lt;br/&gt;
     libraryDependencies ++= Seq(&lt;br/&gt;
       &quot;org.apache.avro&quot;      % &quot;avro&quot;               % &quot;1.4.0&quot;,&lt;br/&gt;
       &quot;org.apache.pig&quot;       % &quot;pig&quot;                % &quot;0.8.0&quot;,&lt;/li&gt;
&lt;/ul&gt;
</comment>
