                            <comment id="17954965" author="chia7712" created="Thu, 29 May 2025 14:46:03 +0000"  >&lt;p&gt;the unit test is shown below&lt;/p&gt;

&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt;
    @Test
    &lt;span class=&quot;code-keyword&quot;&gt;public&lt;/span&gt; void testCommitAsyncShouldCopyOffsets() {
        consumer = newConsumer();
        &lt;span class=&quot;code-keyword&quot;&gt;var&lt;/span&gt; tp = &lt;span class=&quot;code-keyword&quot;&gt;new&lt;/span&gt; TopicPartition(&lt;span class=&quot;code-quote&quot;&gt;&quot;t0&quot;&lt;/span&gt;, 2);
        &lt;span class=&quot;code-keyword&quot;&gt;var&lt;/span&gt; offsets = &lt;span class=&quot;code-keyword&quot;&gt;new&lt;/span&gt; HashMap&amp;lt;TopicPartition, OffsetAndMetadata&amp;gt;();
        offsets.put(tp, &lt;span class=&quot;code-keyword&quot;&gt;new&lt;/span&gt; OffsetAndMetadata(10L));

        markOffsetsReadyForCommitEvent();
        consumer.commitAsync(offsets, &lt;span class=&quot;code-keyword&quot;&gt;null&lt;/span&gt;);

        &lt;span class=&quot;code-keyword&quot;&gt;var&lt;/span&gt; commitEventCaptor = ArgumentCaptor.forClass(AsyncCommitEvent.class);
        verify(applicationEventHandler).add(commitEventCaptor.capture());
        &lt;span class=&quot;code-keyword&quot;&gt;var&lt;/span&gt; commitEvent = commitEventCaptor.getValue();
        assertTrue(commitEvent.offsets().get().containsKey(tp));
        offsets.remove(tp);
        assertTrue(commitEvent.offsets().get().containsKey(tp));
    }
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;</comment>
