/github.com/rspec/rspec-core/blob/master/lib/rspec/core/rake_task.rb#L102<br/>\n[2] http://relishapp.com/rspec/rspec-core/v/2-4/dir/command-line/tag-option \n","author_association":"CONTRIBUTOR","reactions":{"url":"https://api.github.com/repos/rspec/rspec-core/issues/comments/689524/reactions","total_count":0,"+1":0,"-1":0,"laugh":0,"hooray":0,"confused":0,"heart":0,"rocket":0,"eyes":0},"performed_via_github_app":null,"event":"commented","actor":{"login":"dchelimsky","id":1075,"node_id":"MDQ6VXNlcjEwNzU=","avatar_url":"https://avatars.githubusercontent.com/u/1075?v=4","gravatar_id":"","url":"https://api.github.com/users/dchelimsky","html_url":"https://github.com/dchelimsky","followers_url":"https://api.github.com/users/dchelimsky/followers","following_url":"https://api.github.com/users/dchelimsky/following{/other_user}","gists_url":"https://api.github.com/users/dchelimsky/gists{/gist_id}","starred_url":"https://api.github.com/users/dchelimsky/starred{/owner}{/repo}","subscriptions_url":"https://api.github.com/users/dchelimsky/subscriptions","organizations_url":"https://api.github.com/users/dchelimsky/orgs","repos_url":"https://api.github.com/users/dchelimsky/repos","events_url":"https://api.github.com/users/dchelimsky/events{/privacy}","received_events_url":"https://api.github.com/users/dchelimsky/received_events","type":"User","user_view_type":"public","site_admin":false}},{"url":"https://api.github.com/repos/rspec/rspec-core/issues/comments/690954","html_url":"https://github.com/rspec/rspec-core/issues/280#issuecomment-690954","issue_url":"https://api.github.com/repos/rspec/rspec-core/issues/280","id":690954,"node_id":"MDEyOklzc3VlQ29tbWVudDY5MDk1NA==","user":{"login":"paulwalker","id":73511,"node_id":"MDQ6VXNlcjczNTEx","avatar_url":"https://avatars.githubusercontent.com/u/73511?v=4","gravatar_id":"","url":"https://api.github.com/users/paulwalker","html_url":"https://github.com/paulwalker","followers_url":"https://api.github.com/users/paulwalker/followers","follow