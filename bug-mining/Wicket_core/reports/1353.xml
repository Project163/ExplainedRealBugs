                    <description>&lt;div class=&quot;code panel&quot; style=&quot;border-style: solid;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeHeader panelHeader&quot; style=&quot;border-bottom-width: 1px;border-bottom-style: solid;&quot;&gt;&lt;b&gt;HomePage.java&lt;/b&gt;&lt;/div&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt;&lt;span class=&quot;code-keyword&quot;&gt;public&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;class &lt;/span&gt;HomePage &lt;span class=&quot;code-keyword&quot;&gt;extends&lt;/span&gt; WebPage {
    &lt;span class=&quot;code-keyword&quot;&gt;private&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;static&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;final&lt;/span&gt; &lt;span class=&quot;code-object&quot;&gt;long&lt;/span&gt; serialVersionUID = 1L;

    &lt;span class=&quot;code-keyword&quot;&gt;public&lt;/span&gt; HomePage(&lt;span class=&quot;code-keyword&quot;&gt;final&lt;/span&gt; PageParameters parameters) {
        &lt;span class=&quot;code-keyword&quot;&gt;super&lt;/span&gt;(parameters);

        Form&amp;lt;&lt;span class=&quot;code-object&quot;&gt;Void&lt;/span&gt;&amp;gt; form = &lt;span class=&quot;code-keyword&quot;&gt;new&lt;/span&gt; Form&amp;lt;&amp;gt;(&lt;span class=&quot;code-quote&quot;&gt;&quot;form&quot;&lt;/span&gt;);
        form.setMultipart(&lt;span class=&quot;code-keyword&quot;&gt;true&lt;/span&gt;);
        add(form);

        Button components = Button.onSubmit(&lt;span class=&quot;code-quote&quot;&gt;&quot;button&quot;&lt;/span&gt;, &lt;span class=&quot;code-keyword&quot;&gt;this&lt;/span&gt;::buttonOnSubmit);
        form.add(components);
    }

    &lt;span class=&quot;code-keyword&quot;&gt;private&lt;/span&gt; void buttonOnSubmit(Button button) {
        &lt;span class=&quot;code-object&quot;&gt;System&lt;/span&gt;.out.println(&lt;span class=&quot;code-quote&quot;&gt;&quot;test&quot;&lt;/span&gt;);
    }

}
&lt;/pre&gt;
