+                LottieAnimation(
+                    composition,
+                    { 0.15f },
+                    contentScale = ContentScale.Crop,
+                    renderMode = renderMode,
+                    modifier = Modifier
+                        .size(100.dp, 100.dp)
+                )
+
+                LottieAnimation(
+                    composition,
+                    { 0.5f },
+                    contentScale = ContentScale.Crop,
+                    renderMode = renderMode,
+                    modifier = Modifier
+                        .size(100.dp, 100.dp)
+                )
+
+                LottieAnimation(
+                    composition,
+                    { 0.9f },
+                    contentScale = ContentScale.Crop,
+                    renderMode = renderMode,
+                    modifier = Modifier
+                        .size(100.dp, 100.dp)
+                )
+                LottieAnimation(
+                    composition,
+                    { 1f },
+                    contentScale = ContentScale.Crop,
+                    renderMode = renderMode,
+                    modifier = Modifier
+                        .size(100.dp, 100.dp)
+                )
+            }
+        }
+    }
