                                        <assignee username="-1">Unassigned</assignee>
                                    <reporter username="ritchiem">Martin Ritchie</reporter>
                        <labels>
                    </labels>
                <created>Fri, 29 Sep 2006 09:17:08 +0000</created>
                <updated>Mon, 7 Jul 2008 10:50:06 +0000</updated>
                            <resolved>Tue, 3 Oct 2006 14:52:12 +0000</resolved>
