+      	Beta []MyOtherType `json:"beta"`
+      	Delta MyInteger `json:"delta,omitempty"`
+      	Epsilon []custom.MyAlternateType `json:"epsilon"`
+      	Gamma fred.MyAlternateInteger `json:"gamma,omitempty"`
+      	Meta MyType `json:"meta,omitempty"`
