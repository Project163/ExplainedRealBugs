+      # P0
+      - type: string 
+        enum: 
+          - 'CONST1'
+          - 'CONST2'
+      # P1
+      - type: string
+        enum: 
+          - 'CONST3'
+          - 'CONST4'
+  tupleThingWithAdditionalItems:
+    type: array
+    items:
+      # P0
+      - type: string 
+      # P1
+      - type: integer
+        maximum: 10
+    additionalItems:
+      # Additional item type #1
+      type: integer
+      enum:
+      - 1
+      - 2
+  tupleThingWithNoAdditionalItems:
