+      summary: many model variations
+      description: Used to see if a codegen can render all the possible enum categories
+      tags:
+        - testcgen
+      responses:
+        default:
+          description: Generic Out
+definitions:
+  trueTupleThing:
+    type: array
+    additionalItems: true
+    items:
+      - type: number 
+      - type: string 
+  falseTupleThing:
+    type: array
+    additionalItems: false
+    items:
+      - type: number 
+      - type: string 
+  tupleThing:
+    type: array
