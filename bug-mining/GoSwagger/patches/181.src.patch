+			// generate a unique name
+			newName := uniqifyName(opts.Swagger().Definitions, nameFromRef(entry.Ref))
+			if swspec.Debug {
+				log.Printf("new name for [%s]: %s", strings.Join(entry.Keys, ", "), newName)
+			}
