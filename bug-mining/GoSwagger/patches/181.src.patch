+	for key, schRef := range opts.Spec.references.schemas {
+		if entry, ok := collected[schRef.String()]; ok {
+			entry.Keys = append(entry.Keys, key)
+			collected[schRef.String()] = entry
+		} else {
+			collected[schRef.String()] = refRevIdx{
+				Ref:  schRef,
+				Keys: []string{key},
