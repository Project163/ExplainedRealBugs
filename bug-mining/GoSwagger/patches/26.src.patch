+func specResolver(t testing.TB, path string) (*spec.Document, *typeResolver, error) {
+	tlb, err := spec.Load(path)
+	if err != nil {
+		return nil, nil, err
+	}
+	return tlb, &typeResolver{
+		Doc:           tlb,
+		ModelsPackage: "models",
+	}, nil
+}
