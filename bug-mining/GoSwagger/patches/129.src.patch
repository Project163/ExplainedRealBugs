+
+// Serve the api
+func (s *Server) Serve() error {
+	domainSocket := &graceful.Server{Server: new(http.Server)}
+	domainSocket.Handler = s.handler
+	domSockListener, err := net.Listen("unix", string(s.SocketPath))
+	if err != nil {
+		return err
+	}
+
+	log.Printf("serving todo list at unix://%s", s.SocketPath)
+	go func() {
+		if err := domainSocket.Serve(domSockListener); err != nil {
+			log.Fatalln(err)
+		}
+	}()
+
+	httpServer := &graceful.Server{Server: new(http.Server)}
+	httpServer.Handler = s.handler
+
+	listener, err := net.Listen("tcp", fmt.Sprintf("%s:%d", s.Host, s.Port))
+	if err != nil {
+		return err
+	}
+
+	log.Printf("serving todo list at http://%s", listener.Addr())
+	go func() {
+		if err := httpServer.Serve(tcpKeepAliveListener{listener.(*net.TCPListener)}); err != nil {
