+		}
+	}()
+
+	httpsServer := &graceful.Server{Server: new(http.Server)}
+	httpsServer.Handler = s.handler
+	httpsServer.TLSConfig = new(tls.Config)
+	httpsServer.TLSConfig.NextProtos = []string{"http/1.1"}
