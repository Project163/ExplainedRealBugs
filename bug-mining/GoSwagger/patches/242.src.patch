+        $ref: "#/definitions/audit"
+
+  quota:
+     title: Service Unit Quota
+     description: quota details with values
+     type: object
+     properties:
+       min:
+         title: Minimum quota value
+         type: number
+         format: double
+         minimum: 0.0
+         example: 1.0
+       max:
+         title: Maximum quota value
+         type: number
+         format: double
+         minimum: 0.0
+         example: 1.0
+       default:
+         title: Default quota value
+         type: number
+         format: double
+         minimum: 0.0
+         example: 1.0
