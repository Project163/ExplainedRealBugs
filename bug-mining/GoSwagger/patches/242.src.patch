+         type: string
+         example: 2018-02-01
+      deactivated_at:
+         title: Service Unit Cost Deactivation Timestamp
+         description: >
+             service unit deactivation date (exclusive) as YYYY-MM-DD with respect to the billing period
+             - omitted if empty or the end of the billing period
+         type: string
+         example: 2018-03-01
