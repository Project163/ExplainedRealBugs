+  audit:
+    title: Audit
+    description: Audit trail details
+    type: object
+    properties:
+      created_at:
+        title: Created At
+        description: timestamp when the record was created
+        type: string
+        format: date-time
+        readOnly: true
+      created_by:
+        title: Created By
+        description: entity that created the record
+        type: string
+        readOnly: true
+      updated_at:
+        title: Updated At
+        description: timestamp when the record was updated
+        type: string
+        format: date-time
+        readOnly: true
+      updated_by:
+        title: Updated By
+        description: entity that updated the record
+        type: string
+        readOnly: true
