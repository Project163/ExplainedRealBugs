+  error:
+    title: Error
+    description: |
+      An error provides information about why the request failed.
+    type: object
+    required:
+      - code
+      - message
+    properties:
+      code:
+        title: Code
+        description: |
+            Code identifies a specific type of error.
+        type: string
+      message:
+        title: Message
+        description: |
+          The message provides details about the failure.
+        type: string
+
+responses:
