+
+  Ulimit:
+    type: "object"
+    description: "A list of resource limits"
+    properties:
+      Name:
+        description: "Name of ulimit"
+        type: "string"
+      Soft:
+        description: "Soft limit"
+        type: "integer"
+      Hard:
+        description: "Hard limit"
+        type: "integer"
+
+  Container:
+    description: |
+      an array of Container contains response of Engine API:
+      GET "/containers/json"
+    type: "object"
+    properties:
