+        description: "Created time of container in daemon."
+        type: "integer"
+        format: "int64"
+      SizeRw:
+        type: "integer"
+        format: "int64"
+      SizeRootFs:
+        type: "integer"
+        format: "int64"
+      Labels:
+        type: "object"
+        additionalProperties:
+          type: "string"
+      State:
