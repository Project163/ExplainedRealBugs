+package groovy.bugs
+
+import org.junit.Test
+
+import static groovy.test.GroovyAssert.assertScript
+
+final class Groovy9618 {
+
+    @Test
+    void testGetPropertyFromOuterClassOfSuper() {
+        assertScript '''
+            class A {
+              private static X = 1
+              static getX() { 2 }
+              static class B { }
+            }
+
+            class C extends A.B {
+              def m() {
+                return X
+              }
+            }
+
+            assert new C().m() == 2
+        '''
+    }
+}
