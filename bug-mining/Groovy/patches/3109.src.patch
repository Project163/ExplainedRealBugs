+package groovy.bugs
+
+class Groovy7920Bug extends GroovyTestCase {
+    void testGetAtViaInterface() {
+        assertScript '''
+            interface Foo {}
+            class Bar implements Foo {}
+            @groovy.transform.CompileStatic
+            class TestGroovy {
+                static void test() { assert new TestGroovy()[new Bar()] == 42 }
+                def getAt(Foo x) { 42 }
+            }
+            TestGroovy.test()
+        '''
+    }
+
+    void testGetAtViaSuper() {
+        assertScript '''
+            class Foo {}
+            class Bar extends Foo {}
+            @groovy.transform.CompileStatic
+            class TestGroovy {
+                static void test() { assert new TestGroovy()[new Bar()] == 42 }
+                def getAt(Foo x) { 42 }
+            }
+            TestGroovy.test()
+        '''
+    }
+
+    void testGetAtNonNumeric() {
+        assertScript '''
+            @groovy.transform.CompileStatic
+            class TestGroovy {
+                static void test() { assert new TestGroovy()[3] == 42 }
+                def getAt(def x) { 42 }
+            }
+            TestGroovy.test()
+        '''
+    }
+}
