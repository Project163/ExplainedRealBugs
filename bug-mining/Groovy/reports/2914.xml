    +            interface SomeInterface &lt;/p&gt;
{
    +                void someInterfaceMethod()
    +            }
&lt;p&gt;    +&lt;br/&gt;
    +            abstract class AbstractSuperClass implements SomeInterface {}&lt;br/&gt;
