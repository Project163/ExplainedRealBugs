                            <comment id="17571648" author="paulk" created="Tue, 26 Jul 2022 22:51:19 +0000"  >&lt;p&gt;Pseudo-properties (AKA JavaBean properties) are also affected:&lt;/p&gt;
&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt;
&lt;span class=&quot;code-keyword&quot;&gt;class &lt;/span&gt;Base {
    &lt;span class=&quot;code-object&quot;&gt;String&lt;/span&gt; getA() { &lt;span class=&quot;code-quote&quot;&gt;&apos;base a&apos;&lt;/span&gt; }
    &lt;span class=&quot;code-object&quot;&gt;String&lt;/span&gt; getC() { &lt;span class=&quot;code-quote&quot;&gt;&apos;base c&apos;&lt;/span&gt; }
}

@groovy.transform.ToString(includeSuperProperties=&lt;span class=&quot;code-keyword&quot;&gt;true&lt;/span&gt;, allProperties=&lt;span class=&quot;code-keyword&quot;&gt;true&lt;/span&gt;)
&lt;span class=&quot;code-keyword&quot;&gt;class &lt;/span&gt;Child &lt;span class=&quot;code-keyword&quot;&gt;extends&lt;/span&gt; Base {
    &lt;span class=&quot;code-object&quot;&gt;String&lt;/span&gt; getA() { &lt;span class=&quot;code-quote&quot;&gt;&apos;child a&apos;&lt;/span&gt; }
    &lt;span class=&quot;code-object&quot;&gt;String&lt;/span&gt; getB() { &lt;span class=&quot;code-quote&quot;&gt;&apos;child b&apos;&lt;/span&gt; }
}

println &lt;span class=&quot;code-keyword&quot;&gt;new&lt;/span&gt; Child() &lt;span class=&quot;code-comment&quot;&gt;// Child(child b, base c, child a)&lt;/span&gt;
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;</comment>
