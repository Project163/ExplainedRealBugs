<item>
            <title>[GROOVY-5748] glitch with TypeChecked and generics</title>
                <link>https://issues.apache.org/jira/browse/GROOVY-5748</link>
                <project id="12318123" key="GROOVY">Groovy</project>
                    <description>&lt;p&gt;This code:&lt;/p&gt;
&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt;&lt;span class=&quot;code-comment&quot;&gt;// adapted from:
&lt;/span&gt;&lt;span class=&quot;code-comment&quot;&gt;// http://stackoverflow.com/questions/9383681/how-can-i-design-a-type-safe-stack-in-java-preventing-pops-from-an-empty-list
&lt;/span&gt;
&lt;span class=&quot;code-keyword&quot;&gt;import&lt;/span&gt; groovy.transform.ASTTest
&lt;span class=&quot;code-keyword&quot;&gt;import&lt;/span&gt; groovy.transform.TypeChecked
&lt;span class=&quot;code-keyword&quot;&gt;import&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;static&lt;/span&gt; org.codehaus.groovy.transform.stc.StaticTypesMarker.*
&lt;span class=&quot;code-keyword&quot;&gt;import&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;static&lt;/span&gt; org.codehaus.groovy.ast.ClassHelper.*

&lt;span class=&quot;code-keyword&quot;&gt;interface&lt;/span&gt; IStack&amp;lt;T&amp;gt; {
    INonEmptyStack&amp;lt;T, ? &lt;span class=&quot;code-keyword&quot;&gt;extends&lt;/span&gt; IStack&amp;lt;T&amp;gt;&amp;gt; push(T x)
}

&lt;span class=&quot;code-keyword&quot;&gt;interface&lt;/span&gt; IEmptyStack&amp;lt;T&amp;gt; &lt;span class=&quot;code-keyword&quot;&gt;extends&lt;/span&gt; IStack&amp;lt;T&amp;gt; {
    @Override
    INonEmptyStack&amp;lt;T, IEmptyStack&amp;lt;T&amp;gt;&amp;gt; push(T x)
}

&lt;span class=&quot;code-keyword&quot;&gt;interface&lt;/span&gt; INonEmptyStack&amp;lt;T, TStackBeneath &lt;span class=&quot;code-keyword&quot;&gt;extends&lt;/span&gt; IStack&amp;lt;T&amp;gt;&amp;gt; &lt;span class=&quot;code-keyword&quot;&gt;extends&lt;/span&gt; IStack&amp;lt;T&amp;gt; {
    T getTop()

    TStackBeneath pop()

    @Override
    INonEmptyStack&amp;lt;T, INonEmptyStack&amp;lt;T, TStackBeneath&amp;gt;&amp;gt; push(T x)
}

&lt;span class=&quot;code-keyword&quot;&gt;class &lt;/span&gt;EmptyStack&amp;lt;T&amp;gt; &lt;span class=&quot;code-keyword&quot;&gt;implements&lt;/span&gt; IEmptyStack&amp;lt;T&amp;gt; {
    @Override
    INonEmptyStack&amp;lt;T, IEmptyStack&amp;lt;T&amp;gt;&amp;gt; push(T x) {
        &lt;span class=&quot;code-keyword&quot;&gt;new&lt;/span&gt; NonEmptyStack&amp;lt;T, IEmptyStack&amp;lt;T&amp;gt;&amp;gt;(x, &lt;span class=&quot;code-keyword&quot;&gt;this&lt;/span&gt;)
    }
}

&lt;span class=&quot;code-keyword&quot;&gt;class &lt;/span&gt;NonEmptyStack&amp;lt;T, TStackBeneath &lt;span class=&quot;code-keyword&quot;&gt;extends&lt;/span&gt; IStack&amp;lt;T&amp;gt;&amp;gt;
        &lt;span class=&quot;code-keyword&quot;&gt;implements&lt;/span&gt; INonEmptyStack&amp;lt;T, TStackBeneath&amp;gt; {
    &lt;span class=&quot;code-keyword&quot;&gt;private&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;final&lt;/span&gt; TStackBeneath stackBeneathTop;
    &lt;span class=&quot;code-keyword&quot;&gt;private&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;final&lt;/span&gt; T top

    NonEmptyStack(T top, TStackBeneath stackBeneathTop) {
        &lt;span class=&quot;code-keyword&quot;&gt;this&lt;/span&gt;.top = top
        &lt;span class=&quot;code-keyword&quot;&gt;this&lt;/span&gt;.stackBeneathTop = stackBeneathTop
    }

    @Override
    T getTop() {
        top
    }

    @Override
    TStackBeneath pop() {
        stackBeneathTop
    }

    @Override
    INonEmptyStack&amp;lt;T, INonEmptyStack&amp;lt;T, TStackBeneath&amp;gt;&amp;gt; push(T x) {
        &lt;span class=&quot;code-keyword&quot;&gt;new&lt;/span&gt; NonEmptyStack&amp;lt;T, INonEmptyStack&amp;lt;T, TStackBeneath&amp;gt;&amp;gt;(x, &lt;span class=&quot;code-keyword&quot;&gt;this&lt;/span&gt;)
    }
}

@TypeChecked
def main() {
    &lt;span class=&quot;code-keyword&quot;&gt;final&lt;/span&gt; IStack&amp;lt;&lt;span class=&quot;code-object&quot;&gt;Integer&lt;/span&gt;&amp;gt; stack = &lt;span class=&quot;code-keyword&quot;&gt;new&lt;/span&gt; EmptyStack&amp;lt;&lt;span class=&quot;code-object&quot;&gt;Integer&lt;/span&gt;&amp;gt;()

    @ASTTest(phase=INSTRUCTION_SELECTION, value={
        println &lt;span class=&quot;code-quote&quot;&gt;&apos;oneInteger:&apos;&lt;/span&gt; + node.getNodeMetaData(INFERRED_TYPE)
    })
    def oneInteger = stack.push(1)
    &lt;span class=&quot;code-keyword&quot;&gt;assert&lt;/span&gt; oneInteger.getTop() == 1

    @ASTTest(phase=INSTRUCTION_SELECTION, value={
        println &lt;span class=&quot;code-quote&quot;&gt;&apos;twoIntegers:&apos;&lt;/span&gt; + node.getNodeMetaData(INFERRED_TYPE)
    })
    def twoIntegers = stack.push(1).push(2)
    &lt;span class=&quot;code-keyword&quot;&gt;assert&lt;/span&gt; twoIntegers.getTop() == 2

    @ASTTest(phase=INSTRUCTION_SELECTION, value={
        println &lt;span class=&quot;code-quote&quot;&gt;&apos;oneIntegerAgain:&apos;&lt;/span&gt; + node.getNodeMetaData(INFERRED_TYPE)
    })
    def oneIntegerAgain = stack.push(1).push(2).pop()
    &lt;span class=&quot;code-comment&quot;&gt;// &lt;span class=&quot;code-keyword&quot;&gt;assert&lt;/span&gt; oneIntegerAgain.getTop() == 1 // BOOM!!!!
&lt;/span&gt;}
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;
&lt;p&gt;Gives this output:&lt;/p&gt;
&lt;div class=&quot;preformatted panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;preformattedContent panelContent&quot;&gt;
&lt;pre&gt;oneInteger:INonEmptyStack &amp;lt;java.lang.Integer, IEmptyStack&amp;gt; -&amp;gt; INonEmptyStack &amp;lt;T, TStackBeneath extends IStack &amp;lt;T&amp;gt; -&amp;gt; IStack &amp;lt;T&amp;gt;&amp;gt;
twoIntegers:INonEmptyStack &amp;lt;java.lang.Integer, INonEmptyStack&amp;gt; -&amp;gt; INonEmptyStack &amp;lt;T, TStackBeneath extends IStack &amp;lt;T&amp;gt; -&amp;gt; IStack &amp;lt;T&amp;gt;&amp;gt;
oneIntegerAgain:IStack &amp;lt;INonEmptyStack&amp;gt; -&amp;gt; IStack &amp;lt;T&amp;gt;
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;
&lt;p&gt;and gives this error when the last assert is uncommented:&lt;/p&gt;
&lt;div class=&quot;preformatted panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;preformattedContent panelContent&quot;&gt;
&lt;pre&gt;1 compilation error:
[Static type checking] - Cannot find matching method IStack#getTop(). Please check if the declared type is right and if the method exists.
 at line: 79, column: 12
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;</description>
