<item>
            <title>[GROOVY-9031] Traits using generics generate incorrect stub for Properties</title>
                <link>https://issues.apache.org/jira/browse/GROOVY-9031</link>
                <project id="12318123" key="GROOVY">Groovy</project>
                    <description>&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-groovy&quot;&gt;
&lt;span class=&quot;code-keyword&quot;&gt;trait&lt;/span&gt; Test&amp;lt;V&amp;gt; {
    V value
}

&lt;span class=&quot;code-keyword&quot;&gt;class &lt;/span&gt;TestImpl &lt;span class=&quot;code-keyword&quot;&gt;implements&lt;/span&gt; Test&amp;lt;&lt;span class=&quot;code-object&quot;&gt;String&lt;/span&gt;&amp;gt; { }
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;
&lt;p&gt;Generates a stub like this&lt;/p&gt;
&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-groovy&quot;&gt;
&lt;span class=&quot;code-keyword&quot;&gt;import&lt;/span&gt; java.lang.*;
&lt;span class=&quot;code-keyword&quot;&gt;import&lt;/span&gt; java.io.*;
&lt;span class=&quot;code-keyword&quot;&gt;import&lt;/span&gt; java.net.*;
&lt;span class=&quot;code-keyword&quot;&gt;import&lt;/span&gt; java.util.*;
&lt;span class=&quot;code-keyword&quot;&gt;import&lt;/span&gt; groovy.lang.*;
&lt;span class=&quot;code-keyword&quot;&gt;import&lt;/span&gt; groovy.util.*;

public &lt;span class=&quot;code-keyword&quot;&gt;class &lt;/span&gt;TestImpl
  &lt;span class=&quot;code-keyword&quot;&gt;extends&lt;/span&gt; java.lang.&lt;span class=&quot;code-object&quot;&gt;Object&lt;/span&gt;  &lt;span class=&quot;code-keyword&quot;&gt;implements&lt;/span&gt;
    Test&amp;lt;java.lang.&lt;span class=&quot;code-object&quot;&gt;String&lt;/span&gt;&amp;gt;,    groovy.lang.GroovyObject {
;
@groovy.transform.Generated() @groovy.transform.Internal() public  groovy.lang.MetaClass getMetaClass() { &lt;span class=&quot;code-keyword&quot;&gt;return&lt;/span&gt; (groovy.lang.MetaClass)&lt;span class=&quot;code-keyword&quot;&gt;null&lt;/span&gt;;}
@groovy.transform.Generated() @groovy.transform.Internal() public  void setMetaClass(groovy.lang.MetaClass mc) { }
@groovy.transform.Generated() @groovy.transform.Internal() public  java.lang.&lt;span class=&quot;code-object&quot;&gt;Object&lt;/span&gt; invokeMethod(java.lang.&lt;span class=&quot;code-object&quot;&gt;String&lt;/span&gt; method, java.lang.&lt;span class=&quot;code-object&quot;&gt;Object&lt;/span&gt; arguments) { &lt;span class=&quot;code-keyword&quot;&gt;return&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;null&lt;/span&gt;;}
@groovy.transform.Generated() @groovy.transform.Internal() public  java.lang.&lt;span class=&quot;code-object&quot;&gt;Object&lt;/span&gt; getProperty(java.lang.&lt;span class=&quot;code-object&quot;&gt;String&lt;/span&gt; property) { &lt;span class=&quot;code-keyword&quot;&gt;return&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;null&lt;/span&gt;;}
@groovy.transform.Generated() @groovy.transform.Internal() public  void setProperty(java.lang.&lt;span class=&quot;code-object&quot;&gt;String&lt;/span&gt; property, java.lang.&lt;span class=&quot;code-object&quot;&gt;Object&lt;/span&gt; value) { }
public  V getValue() { &lt;span class=&quot;code-keyword&quot;&gt;return&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;null&lt;/span&gt;;}
public  void setValue(V value) { }
}
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;
&lt;p&gt;Note that &lt;tt&gt;value&lt;/tt&gt; has type &lt;tt&gt;V&lt;/tt&gt; instead of type &lt;tt&gt;String&lt;/tt&gt; and the generic reference isn&apos;t declared in the class either.&lt;/p&gt;</description>
