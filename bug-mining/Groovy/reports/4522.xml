  print list.collect(C.&amp;amp;getFoo) &lt;span class=&quot;code-comment&quot;&gt;// Cannot find matching method C#getFoo
&lt;/span&gt;}
test([&lt;span class=&quot;code-keyword&quot;&gt;new&lt;/span&gt; C(foo:&lt;span class=&quot;code-quote&quot;&gt;&apos;bar&apos;&lt;/span&gt;), &lt;span class=&quot;code-keyword&quot;&gt;new&lt;/span&gt; C(foo:&lt;span class=&quot;code-quote&quot;&gt;&apos;baz&apos;&lt;/span&gt;)])
