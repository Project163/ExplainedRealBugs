                            <comment id="17150603" author="emilles" created="Thu, 2 Jul 2020 21:55:00 +0000"  >&lt;p&gt;Here is a smaller example:&lt;/p&gt;
&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-groovy&quot;&gt;
&lt;span class=&quot;code-keyword&quot;&gt;import&lt;/span&gt; groovy.transform.*
&lt;span class=&quot;code-keyword&quot;&gt;import&lt;/span&gt; groovy.transform.stc.*

&lt;span class=&quot;code-keyword&quot;&gt;class &lt;/span&gt;Help9597 {
  &lt;span class=&quot;code-keyword&quot;&gt;def&lt;/span&gt; &amp;lt;T&amp;gt; void proc(Collection&amp;lt;T&amp;gt; values,
      @ClosureParams(FirstParam.FirstGenericType) Closure&amp;lt;&lt;span class=&quot;code-object&quot;&gt;String&lt;/span&gt;&amp;gt; block) {
  }
}

@TypeChecked
&lt;span class=&quot;code-keyword&quot;&gt;class &lt;/span&gt;Main9597 {
  List&amp;lt;&lt;span class=&quot;code-object&quot;&gt;Integer&lt;/span&gt;&amp;gt; list
  void test(Help9597 h) {
    h.proc(&lt;span class=&quot;code-keyword&quot;&gt;this&lt;/span&gt;.list) { it.toBigDecimal().toString() } &lt;span class=&quot;code-comment&quot;&gt;// works
&lt;/span&gt;    h.with {
      proc(&lt;span class=&quot;code-keyword&quot;&gt;this&lt;/span&gt;.list) { it.toBigDecimal().toString() } &lt;span class=&quot;code-comment&quot;&gt;// error
&lt;/span&gt;    }
  }
}
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;</comment>
