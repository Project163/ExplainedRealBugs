42 astore_1
43 aload_1
44 pop
45 aconst_null
46 astore_2
47 aload_1
48 invokeinterface #45 &amp;lt;java/util/List/iterator()Ljava/util/Iterator;&amp;gt; count 1
&lt;span class=&quot;code-comment&quot;&gt;/*53 invokestatic #50 &amp;lt;org/codehaus/groovy/runtime/DefaultGroovyMethods/iterator(Ljava/lang/&lt;span class=&quot;code-object&quot;&gt;Object&lt;/span&gt;;)Ljava/util/Iterator;&amp;gt;*/&lt;/span&gt;
56 astore_3
57 aload_3
58 invokeinterface #56 &amp;lt;java/util/Iterator/hasNext()Z&amp;gt; count 1
63 ifeq 84 (+21)
66 aload_3
67 invokeinterface #60 &amp;lt;java/util/Iterator/next()Ljava/lang/&lt;span class=&quot;code-object&quot;&gt;Object&lt;/span&gt;;&amp;gt; count 1
72 astore_2
73 ldc #2 &amp;lt;IteratorTest&amp;gt;
75 aload_2
76 invokestatic #64 &amp;lt;org/codehaus/groovy/runtime/DefaultGroovyMethods/println(Ljava/lang/&lt;span class=&quot;code-object&quot;&gt;Object&lt;/span&gt;;Ljava/lang/&lt;span class=&quot;code-object&quot;&gt;Object&lt;/span&gt;;)V&amp;gt;
79 aconst_null
80 pop
81 &lt;span class=&quot;code-keyword&quot;&gt;goto&lt;/span&gt; 57 (-24)
84 &lt;span class=&quot;code-keyword&quot;&gt;return&lt;/span&gt;
