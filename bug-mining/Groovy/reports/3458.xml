                            <comment id="14477102" author="dmovchinn" created="Wed, 4 Mar 2015 11:29:22 +0000"  >&lt;p&gt;Also, you can see that&lt;/p&gt;
&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt;&lt;span class=&quot;code-keyword&quot;&gt;package&lt;/span&gt; org.dimitrovchi.groovybugs;

&lt;span class=&quot;code-keyword&quot;&gt;import&lt;/span&gt; groovy.lang.GroovyShell;
&lt;span class=&quot;code-keyword&quot;&gt;import&lt;/span&gt; org.codehaus.groovy.control.CompilerConfiguration;
&lt;span class=&quot;code-keyword&quot;&gt;import&lt;/span&gt; org.codehaus.groovy.control.customizers.ImportCustomizer;
&lt;span class=&quot;code-keyword&quot;&gt;import&lt;/span&gt; org.junit.Assert;
&lt;span class=&quot;code-keyword&quot;&gt;import&lt;/span&gt; org.junit.Test;

/**
 * @author Dmitry Ovchinnikov
 */
&lt;span class=&quot;code-keyword&quot;&gt;public&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;class &lt;/span&gt;MapToProxyTest {

    &lt;span class=&quot;code-keyword&quot;&gt;private&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;final&lt;/span&gt; GroovyShell shell;

    &lt;span class=&quot;code-keyword&quot;&gt;public&lt;/span&gt; MapToProxyTest() {
        &lt;span class=&quot;code-keyword&quot;&gt;final&lt;/span&gt; CompilerConfiguration cc = &lt;span class=&quot;code-keyword&quot;&gt;new&lt;/span&gt; CompilerConfiguration();
        cc.setTargetBytecode(CompilerConfiguration.JDK8);
        cc.addCompilationCustomizers(&lt;span class=&quot;code-keyword&quot;&gt;new&lt;/span&gt; ImportCustomizer().addStarImports(MapToProxyTest.&lt;span class=&quot;code-keyword&quot;&gt;class.&lt;/span&gt;getPackage().getName()));
        shell = &lt;span class=&quot;code-keyword&quot;&gt;new&lt;/span&gt; GroovyShell(cc);
    }

    @Test
    &lt;span class=&quot;code-keyword&quot;&gt;public&lt;/span&gt; void testMapToProxy() {
        &lt;span class=&quot;code-keyword&quot;&gt;final&lt;/span&gt; SomeInterface si = (SomeInterface) shell.evaluate(&lt;span class=&quot;code-quote&quot;&gt;&quot;[x: {10}, y: {20}] as MapToProxyTest.SomeInterface&quot;&lt;/span&gt;);
        Assert.assertEquals(20, si.y());
        Assert.assertEquals(10, si.x()); &lt;span class=&quot;code-comment&quot;&gt;// assertion error: si.x() == 1
&lt;/span&gt;    }

    &lt;span class=&quot;code-keyword&quot;&gt;public&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;interface&lt;/span&gt; SomeInterface {

        &lt;span class=&quot;code-keyword&quot;&gt;default&lt;/span&gt; &lt;span class=&quot;code-object&quot;&gt;int&lt;/span&gt; x() {
            &lt;span class=&quot;code-keyword&quot;&gt;return&lt;/span&gt; 1;
        }

        &lt;span class=&quot;code-object&quot;&gt;int&lt;/span&gt; y();
    }
}
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;

&lt;p&gt;produces the same result.&lt;/p&gt;</comment>
