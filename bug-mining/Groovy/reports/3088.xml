      long register = 0xfac432b10cd5e44aL  &lt;/p&gt;

&lt;p&gt;      &lt;span class=&quot;error&quot;&gt;&amp;#91;1,2,3&amp;#93;&lt;/span&gt;.each &lt;/p&gt;
{ int element -&amp;gt; 
        int t = (int) (register &amp;gt;&amp;gt;&amp;gt; 56 ^ (long) element) &amp;amp; 0xff
        println t
      }
&lt;p&gt;  &lt;br/&gt;
    }&lt;br/&gt;
    ```&lt;/p&gt;

