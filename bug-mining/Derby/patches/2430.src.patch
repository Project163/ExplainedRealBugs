+                null, // TableName
+                td, // TableDescriptor
+                Collections.EMPTY_LIST,
+                Boolean.FALSE,
+                cm);
+
+        QueryTreeNode vtiNode;
+
+        if (correlationName != null) {
+            vtiNode = getNodeFactory().getNode(C_NodeTypes.FROM_VTI,
+                    newNode,
