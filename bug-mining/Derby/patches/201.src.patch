+public class Stream {
+    
+    public static void main(String[] args){
+	
+	Connection conn = null;
+
+	try{
+	    ij.getPropertyArg(args);
+	    conn = ij.startJBMS();
+	    
+	    createTestTables(conn);
+	    executeTests(conn);
+	    dropTestTables(conn);
+	    
+	}catch(Throwable t){
+	    t.printStackTrace();
+	    
+	}finally{
+	    if(conn != null){
