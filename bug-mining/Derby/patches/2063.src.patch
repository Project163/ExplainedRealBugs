-        // year divisible by four that was not a leap year (like 2100, 2200,
-        // 2300, 2500, ...).
-        assertDateString("2100-05-17 14:10:44.701 GMT");
-        assertDateString("2927-06-07 00:00:00.000 GMT");
-        assertDateString("9999-12-31 23:59:59.999 GMT");
-
