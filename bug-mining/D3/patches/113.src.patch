+  forward.invert = function(x, y) {
+    var ρ0_y = G - y;
+    return [
+      Math.atan2(x, ρ0_y) / n,
+      G - d3_sgn(n) * Math.sqrt(x * x + ρ0_y * ρ0_y)
+    ];
+  };
