&lt;span class=&quot;code-keyword&quot;&gt;INSERT&lt;/span&gt; OVERWRITE database5_t0(c0, c1) &lt;span class=&quot;code-keyword&quot;&gt;VALUES&lt;/span&gt;(&lt;span class=&quot;code-quote&quot;&gt;&apos;(LZ *3&apos;&lt;/span&gt;, 2135917226)
&lt;span class=&quot;code-keyword&quot;&gt;SELECT&lt;/span&gt; database5_t0.c0 &lt;span class=&quot;code-keyword&quot;&gt;AS&lt;/span&gt; ref0 &lt;span class=&quot;code-keyword&quot;&gt;FROM&lt;/span&gt; database5_t0 &lt;span class=&quot;code-keyword&quot;&gt;WHERE&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;CAST&lt;/span&gt; (database5_t0.c0 &lt;span class=&quot;code-keyword&quot;&gt;AS&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;BOOLEAN&lt;/span&gt;)
&lt;/pre&gt;
