&lt;p&gt;    &lt;a href=&quot;https://github.com/apache/flink/pull/546#discussion_r27410240&quot; class=&quot;external-link&quot; target=&quot;_blank&quot; rel=&quot;nofollow noopener&quot;&gt;https://github.com/apache/flink/pull/546#discussion_r27410240&lt;/a&gt;&lt;/p&gt;

&lt;p&gt;    &amp;#8212; Diff: flink-runtime/src/main/java/org/apache/flink/runtime/io/disk/iomanager/IOManagerAsync.java &amp;#8212;&lt;br/&gt;
    @@ -43,7 +45,10 @@&lt;/p&gt;

&lt;p&gt;     	/** Flag to signify that the IOManager has been shut down already */&lt;br/&gt;
     	private final AtomicBoolean isShutdown = new AtomicBoolean();&lt;/p&gt;
&lt;ul class=&quot;alternate&quot; type=&quot;square&quot;&gt;
	&lt;li&gt;&lt;p&gt;    +&lt;br/&gt;
    +	/** Logging */&lt;br/&gt;
