		StreamExecutionEnvironment env = StreamExecutionEnvironment.getExecutionEnvironment();
		env.setParallelism(2);

		DataStream&amp;lt;&lt;span class=&quot;code-object&quot;&gt;String&lt;/span&gt;&amp;gt; input = env.fromElements(
				&lt;span class=&quot;code-quote&quot;&gt;&quot;a-1&quot;&lt;/span&gt;, &lt;span class=&quot;code-quote&quot;&gt;&quot;a-2&quot;&lt;/span&gt;, &lt;span class=&quot;code-quote&quot;&gt;&quot;a-3&quot;&lt;/span&gt;, &lt;span class=&quot;code-quote&quot;&gt;&quot;a-4&quot;&lt;/span&gt;, &lt;span class=&quot;code-quote&quot;&gt;&quot;b-1&quot;&lt;/span&gt;, &lt;span class=&quot;code-quote&quot;&gt;&quot;b-2&quot;&lt;/span&gt;, &lt;span class=&quot;code-quote&quot;&gt;&quot;b-3&quot;&lt;/span&gt;
