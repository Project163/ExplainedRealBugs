        &lt;span class=&quot;code-quote&quot;&gt;&quot;type&quot;&lt;/span&gt; : &lt;span class=&quot;code-quote&quot;&gt;&quot;UNKNOWN&quot;&lt;/span&gt;
      },
      &lt;span class=&quot;code-quote&quot;&gt;&quot;damBehavior&quot;&lt;/span&gt; : &lt;span class=&quot;code-quote&quot;&gt;&quot;PIPELINED&quot;&lt;/span&gt;,
      &lt;span class=&quot;code-quote&quot;&gt;&quot;priority&quot;&lt;/span&gt; : 0
    } ],
    &lt;span class=&quot;code-quote&quot;&gt;&quot;outputType&quot;&lt;/span&gt; : &lt;span class=&quot;code-quote&quot;&gt;&quot;ROW&amp;lt;&lt;span class=&quot;code-quote&quot;&gt;`f0`&lt;/span&gt; INT, &lt;span class=&quot;code-quote&quot;&gt;`f1`&lt;/span&gt; VARCHAR(2147483647)&amp;gt;&quot;&lt;/span&gt;,
    &lt;span class=&quot;code-quote&quot;&gt;&quot;description&quot;&lt;/span&gt; : &lt;span class=&quot;code-quote&quot;&gt;&quot;Sink(table=[default_catalog.default_database.bar], fields=[f0, f1])&quot;&lt;/span&gt;
  } ],
  &lt;span class=&quot;code-quote&quot;&gt;&quot;edges&quot;&lt;/span&gt; : [ {
