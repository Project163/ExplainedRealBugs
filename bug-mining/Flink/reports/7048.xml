      &lt;span class=&quot;code-quote&quot;&gt;&quot;table&quot;&lt;/span&gt; : {
        &lt;span class=&quot;code-quote&quot;&gt;&quot;identifier&quot;&lt;/span&gt; : &lt;span class=&quot;code-quote&quot;&gt;&quot;&lt;span class=&quot;code-quote&quot;&gt;`default_catalog`&lt;/span&gt;.&lt;span class=&quot;code-quote&quot;&gt;`default_database`&lt;/span&gt;.&lt;span class=&quot;code-quote&quot;&gt;`bar`&lt;/span&gt;&quot;&lt;/span&gt;
      }
    },
    &lt;span class=&quot;code-quote&quot;&gt;&quot;inputChangelogMode&quot;&lt;/span&gt; : [ &lt;span class=&quot;code-quote&quot;&gt;&quot;INSERT&quot;&lt;/span&gt; ],
    &lt;span class=&quot;code-quote&quot;&gt;&quot;inputProperties&quot;&lt;/span&gt; : [ {
