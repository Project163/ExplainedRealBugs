        name = &lt;span class=&quot;code-quote&quot;&gt;&quot;stream-exec-sink&quot;&lt;/span&gt;,
        version = 2,
        consumedOptions = {
            &lt;span class=&quot;code-quote&quot;&gt;&quot;table.exec.sink.not-&lt;span class=&quot;code-keyword&quot;&gt;null&lt;/span&gt;-enforcer&quot;&lt;/span&gt;,
            &lt;span class=&quot;code-quote&quot;&gt;&quot;table.exec.sink.type-length-enforcer&quot;&lt;/span&gt;,
            &lt;span class=&quot;code-quote&quot;&gt;&quot;table.exec.sink.upsert-materialize&quot;&lt;/span&gt;,
            &lt;span class=&quot;code-quote&quot;&gt;&quot;table.exec.sink.keyed-shuffle&quot;&lt;/span&gt;
        },
        producedTransformations = {
            CommonExecSink.CONSTRAINT_VALIDATOR_TRANSFORMATION,
            CommonExecSink.PARTITIONER_TRANSFORMATION,
            CommonExecSink.UPSERT_MATERIALIZE_TRANSFORMATION,
            CommonExecSink.TIMESTAMP_INSERTER_TRANSFORMATION,
