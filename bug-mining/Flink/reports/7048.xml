    &lt;span class=&quot;code-quote&quot;&gt;&quot;configuration&quot;&lt;/span&gt; : {
      &lt;span class=&quot;code-quote&quot;&gt;&quot;table.exec.sink.keyed-shuffle&quot;&lt;/span&gt; : &lt;span class=&quot;code-quote&quot;&gt;&quot;AUTO&quot;&lt;/span&gt;,
      &lt;span class=&quot;code-quote&quot;&gt;&quot;table.exec.sink.not-&lt;span class=&quot;code-keyword&quot;&gt;null&lt;/span&gt;-enforcer&quot;&lt;/span&gt; : &lt;span class=&quot;code-quote&quot;&gt;&quot;ERROR&quot;&lt;/span&gt;,
      &lt;span class=&quot;code-quote&quot;&gt;&quot;table.exec.sink.type-length-enforcer&quot;&lt;/span&gt; : &lt;span class=&quot;code-quote&quot;&gt;&quot;IGNORE&quot;&lt;/span&gt;,
      &lt;span class=&quot;code-quote&quot;&gt;&quot;table.exec.sink.upsert-materialize&quot;&lt;/span&gt; : &lt;span class=&quot;code-quote&quot;&gt;&quot;AUTO&quot;&lt;/span&gt;
    },
