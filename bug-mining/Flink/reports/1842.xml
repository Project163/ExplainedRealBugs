    $ mvn dependency:tree -pl flink-scala -Pscala-2.11 | grep quasi&lt;br/&gt;
    &lt;span class=&quot;error&quot;&gt;&amp;#91;INFO&amp;#93;&lt;/span&gt; +- org.scalamacros:quasiquotes_2.10:jar:2.1.0:compile&lt;br/&gt;
    $ mvn dependency:tree -pl flink-scala -Pscala-2.10 | grep quasi&lt;br/&gt;
    &lt;span class=&quot;error&quot;&gt;&amp;#91;INFO&amp;#93;&lt;/span&gt; +- org.scalamacros:quasiquotes_2.10:jar:2.1.0:compile&lt;br/&gt;
