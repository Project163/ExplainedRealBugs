+        YarnJobClusterEntrypoint yarnJobClusterEntrypoint =
+                new YarnJobClusterEntrypoint(configuration);
+
+        ClusterEntrypoint.runClusterEntrypoint(yarnJobClusterEntrypoint);
+    }
