+            jobGraph.addJar(new org.apache.flink.core.fs.Path(testingJar.toURI()));
+            try (ClusterClient<ApplicationId> clusterClient =
+                    yarnClusterDescriptor
+                            .deployJobCluster(clusterSpecification, jobGraph, false)
+                            .getClusterClient()) {
