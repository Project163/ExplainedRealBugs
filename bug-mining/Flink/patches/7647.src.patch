+                for (DistributedCache.DistributedCacheEntry entry :
+                        jobGraph.getUserArtifacts().values()) {
+                    assertThat(Utils.isRemotePath(entry.filePath)).isTrue();
+                }
+
+                ApplicationId applicationId = clusterClient.getClusterId();
