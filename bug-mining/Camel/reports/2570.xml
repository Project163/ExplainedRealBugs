                <project id="12311211" key="CAMEL">Camel</project>
                    <description>&lt;p&gt;&lt;a href=&quot;http://camel.465427.n5.nabble.com/camel-rabbitmq-routing-issue-tp5744371p5744374.html&quot; class=&quot;external-link&quot; target=&quot;_blank&quot; rel=&quot;nofollow noopener&quot;&gt;An user complains about camel route keeping sending the message to the same queue&lt;/a&gt; when using the camel-rabbitmq as a proxy.&lt;/p&gt;
&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt;&amp;lt;camelContext xmlns=&lt;span class=&quot;code-quote&quot;&gt;&quot;http:&lt;span class=&quot;code-comment&quot;&gt;//camel.apache.org/schema/spring&quot;&lt;/span&gt;&amp;gt; 
&lt;/span&gt;&amp;lt;route&amp;gt; 
&amp;lt;from 
uri=&lt;span class=&quot;code-quote&quot;&gt;&quot;rabbitmq:&lt;span class=&quot;code-comment&quot;&gt;//localhost/general.topic?queue=general.queue&amp;amp;amp;routingKey=general.queue&amp;amp;amp;exchangeType=topic&amp;amp;amp;username=guest&amp;amp;amp;password=guest&amp;amp;amp;autoDelete=&lt;span class=&quot;code-keyword&quot;&gt;false&lt;/span&gt;&amp;amp;amp;durable=&lt;span class=&quot;code-keyword&quot;&gt;true&lt;/span&gt;&quot;&lt;/span&gt;
&lt;/span&gt;/&amp;gt; 
&amp;lt;to 
uri=&lt;span class=&quot;code-quote&quot;&gt;&quot;rabbitmq:&lt;span class=&quot;code-comment&quot;&gt;//localhost/scheduler.topic?queue=scheduler.queue&amp;amp;amp;routingKey=scheduler.queue&amp;amp;amp;exchangeType=topic&amp;amp;amp;username=guest&amp;amp;amp;password=guest&amp;amp;amp;autoDelete=&lt;span class=&quot;code-keyword&quot;&gt;false&lt;/span&gt;&amp;amp;amp;durable=&lt;span class=&quot;code-keyword&quot;&gt;true&lt;/span&gt;&quot;&lt;/span&gt;
&lt;/span&gt;/&amp;gt; 
&amp;lt;/route&amp;gt; 
&amp;lt;/camelContext&amp;gt;
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;

