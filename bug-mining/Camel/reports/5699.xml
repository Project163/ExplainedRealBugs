&lt;p&gt;&#160;&lt;/p&gt;

&lt;p&gt;Changing the order to this, fixes:&lt;/p&gt;
&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt;
- from:
    uri: &lt;span class=&quot;code-quote&quot;&gt;&quot;timer:tick&quot;&lt;/span&gt;
    steps:
    - to:
        uri: &lt;span class=&quot;code-quote&quot;&gt;&quot;log:info&quot;&lt;/span&gt;
        parameters:
          showHeaders: &lt;span class=&quot;code-quote&quot;&gt;&quot;&lt;span class=&quot;code-keyword&quot;&gt;true&lt;/span&gt;&quot;&lt;/span&gt; &lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;</description>
