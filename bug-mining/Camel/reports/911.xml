                <project id="12311211" key="CAMEL">Camel</project>
                    <description>&lt;p&gt;See CacheConfiguration.java&lt;/p&gt;

&lt;div class=&quot;code panel&quot; style=&quot;border-style: solid;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeHeader panelHeader&quot; style=&quot;border-bottom-width: 1px;border-bottom-style: solid;&quot;&gt;&lt;b&gt;CacheConfiguration.java&lt;/b&gt;&lt;/div&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt;
 &lt;span class=&quot;code-keyword&quot;&gt;if&lt;/span&gt; (cacheSettings.containsKey(&lt;span class=&quot;code-quote&quot;&gt;&quot;diskExpiryThreadIntervalSeconds&quot;&lt;/span&gt;)) {
