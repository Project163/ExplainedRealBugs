&lt;p&gt;The test method :&lt;/p&gt;
&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt; @Test
    &lt;span class=&quot;code-keyword&quot;&gt;public&lt;/span&gt; void testInsertListOfPojo() {
        assertEquals(0, testCollection.count());
        &lt;span class=&quot;code-object&quot;&gt;Object&lt;/span&gt; result = template.requestBody(&lt;span class=&quot;code-quote&quot;&gt;&quot;direct:insertPojo&quot;&lt;/span&gt;, &lt;span class=&quot;code-keyword&quot;&gt;new&lt;/span&gt; MyPojoTestList().getMyPojoTestArrayList());
        DBObject b = testCollection.find(&lt;span class=&quot;code-keyword&quot;&gt;new&lt;/span&gt; BasicDBObject(&lt;span class=&quot;code-quote&quot;&gt;&quot;_id&quot;&lt;/span&gt;, &lt;span class=&quot;code-quote&quot;&gt;&quot;testInsertPojo1&quot;&lt;/span&gt;)).first();
