                                    <reporter username="cschneider">Christian Schneider</reporter>
                        <labels>
                    </labels>
                <created>Fri, 17 Sep 2010 12:49:39 +0000</created>
                <updated>Tue, 25 Oct 2011 11:35:50 +0000</updated>
                            <resolved>Thu, 6 Jan 2011 09:24:54 +0000</resolved>
