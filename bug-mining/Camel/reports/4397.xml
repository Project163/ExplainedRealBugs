        &lt;a href=&quot;http://www.springframework.org/schema/osgi-compendium/spring-osgi-compendium.xsd&quot; class=&quot;external-link&quot; target=&quot;_blank&quot; rel=&quot;nofollow noopener&quot;&gt;http://www.springframework.org/schema/osgi-compendium/spring-osgi-compendium.xsd&lt;/a&gt;&quot;&amp;gt;&lt;/p&gt;



&lt;p&gt;	&amp;lt;camel:camelContext trace=&quot;true&quot; xmlns=&quot;http://camel.apache.org/schema/osgi&quot;&amp;gt;&lt;/p&gt;

&lt;p&gt;		&amp;lt;camel:route&amp;gt;&lt;br/&gt;
			&amp;lt;camel:from	uri=&quot;file://d:/temp/data/reportincident/?move=d:/temp/backup/${date:now:yyyyMMdd}/${&lt;a href=&quot;file:name.noext&quot; class=&quot;external-link&quot; target=&quot;_blank&quot; rel=&quot;nofollow noopener&quot;&gt;file:name.noext&lt;/a&gt;}.bak&quot;/&amp;gt;&lt;br/&gt;
			&amp;lt;camel:setHeader headerName=&quot;origin&quot;&amp;gt;&lt;br/&gt;
				&amp;lt;camel:constant&amp;gt;file&amp;lt;/camel:constant&amp;gt;&lt;br/&gt;
			&amp;lt;/camel:setHeader&amp;gt;&lt;br/&gt;
			&amp;lt;camel:to uri=&quot;log:org.apache.camel.reportincident?level=DEBUG&quot; /&amp;gt;&lt;br/&gt;
		&amp;lt;/camel:route&amp;gt;&lt;/p&gt;


&lt;p&gt;    &amp;lt;/camel:camelContext&amp;gt;&lt;br/&gt;
&amp;lt;/beans&amp;gt;&lt;/p&gt;</description>
