<item>
            <title>[CASSANDRA-13473] Missing quotes in cassandra-env.ps1</title>
                <link>https://issues.apache.org/jira/browse/CASSANDRA-13473</link>
                <project id="12310865" key="CASSANDRA">Apache Cassandra</project>
                    <description>&lt;p&gt;Cannot start Cassandra on Windows via Powershell if the configuration path contains spaces.&lt;/p&gt;

&lt;div class=&quot;preformatted panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;preformattedContent panelContent&quot;&gt;
&lt;pre&gt;diff --git a/conf/cassandra-env.ps1 b/conf/cassandra-env.ps1
index 806eabc..c2765f8 100644
--- a/conf/cassandra-env.ps1
+++ b/conf/cassandra-env.ps1
@@ -376,9 +376,9 @@ Function SetCassandraEnvironment
         }
     }
 
-    # provides hints to the JIT compiler
-    $env:JVM_OPTS = &quot;$env:JVM_OPTS -XX:CompileCommandFile=$env:CASSANDRA_CONF\hotspot_compiler&quot;
-
+    # provides hints to the JIT compiler
+    $env:JVM_OPTS = &quot;$env:JVM_OPTS -XX:CompileCommandFile=&quot;&quot;$env:CASSANDRA_CONF\hotspot_compiler&quot;&quot;&quot;
+
     # add the jamm javaagent
     if (($env:JVM_VENDOR -ne &quot;OpenJDK&quot;) -or ($env:JVM_VERSION.CompareTo(&quot;1.6.0&quot;) -eq 1) -or
         (($env:JVM_VERSION -eq &quot;1.6.0&quot;) -and ($env:JVM_PATCH_VERSION.CompareTo(&quot;22&quot;) -eq 1)))
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;</description>
