&lt;span class=&quot;code-keyword&quot;&gt;if&lt;/span&gt; (thriftServer != &lt;span class=&quot;code-keyword&quot;&gt;null&lt;/span&gt;)
        {
            thriftServer.stop();
            thriftServer = &lt;span class=&quot;code-keyword&quot;&gt;null&lt;/span&gt;;
        }
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;

&lt;p&gt;startRPCServer does&lt;/p&gt;

&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
