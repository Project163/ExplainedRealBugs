         *\n             * foreach ($autoload['libraries'] as $item)\n             * {\n             *      $this->library($item);\n             * }\n             *\n             */\n            foreach ($autoload['libraries'] as $item => $name)\n            {               \n                if(is_int($item))\n                {\n                    $this->library($name);\n                }\n                else\n                {\n                    $this->library($item, NULL, $name);\n                }\n            }\n```\n","author_association":"NONE","reactions":{"url":"https://api.github.com/repos/bcit-ci/CodeIgniter/issues/comments/89584488/reactions","total_count":0,"+1":0,"-1":0,"laugh":0,"hooray":0,"confused":0,"heart":0,"rocket":0,"eyes":0},"performed_via_github_app":null,"event":"commented","actor":{"login":"adaliszk","id":5189131,"node_id":"MDQ6VXNlcjUxODkxMzE=","avatar_url":"https://avatars.githubusercontent.com/u/5189131?v=4","gravatar_id":"","url":"https://api.github.com/users/adaliszk","html_url":"https://github.com/adaliszk","followers_url":"https://api.github.com/users/adaliszk/followers","following_url":"https://api.github.com/users/adaliszk/following{/other_user}","gists_url":"https://api.github.com/users/adaliszk/gists{/gist_id}","starred_url":"https://api.github.com/users/adaliszk/starred{/owner}{/repo}","subscriptions_url":"https://api.github.com/users/adaliszk/subscriptions","organizations_url":"https://api.github.com/users/adaliszk/orgs","repos_url":"https://api.github.com/users/adaliszk/repos","events_url":"https://api.github.com/users/adaliszk/events{/privacy}","received_events_url":"https://api.github.com/users/adaliszk/received_events","type":"User","user_view_type":"public","site_admin":false}},{"id":273285853,"node_id":"MDExOkNsb3NlZEV2ZW50MjczMjg1ODUz","url":"https://api.github.com/repos/bcit-ci/CodeIgniter/issues/events/273285853","actor":{"login":"adaliszk","id":5189131,"node_id":"MDQ6VXNlcjUxODkxMzE=","avatar_url":"https://avatars.