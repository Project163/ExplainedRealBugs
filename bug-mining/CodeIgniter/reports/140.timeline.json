this->unit->report();\n    }\n\n    function test_named_keys() {\n        $_POST['sports']['nba']['basketball'] = 6; //from http://codeigniter.com/user_guide/libraries/form_validation.html#arraysasfields\n        $_POST['sports']['nba']['another'] = 11;\n\n        $this->load->library('form_validation','','form_validation_named');\n\n        $this->form_validation_named->set_rules('sports[nba]', 'blabla', 'callback__valid_sports');\n\n        $this->unit->run($this->form_validation_named->run(), FALSE, 'Form validation should fail - test_named_keys()');\n    }\n\n    function test_keys(){\n        $_POST['sports']['nba'][] = 6;\n        $_POST['sports']['nba'][] = 11;\n\n        $this->load->library('form_validation','','form_validation_');\n        $this->form_validation_->set_rules('sports[nba]', 'blabla', 'callback__valid_sports');\n\n        $this->unit->run($this->form_validation_->run(), FALSE, 'Form validation should fail - test_keys()');\n    }\n\n    function _valid_sports($arr) {\n        foreach (debug_backtrace(false) as $call) {\n            if ($call['class'] == 'Test' && $call['function'] != '_valid_sports') {\n                echo '<br>Validating this with callback. Test: ',$call['function'],' Line:', $call['line'],'.';\n                echo \"<br>\";\n                print_r($arr);\n                break;\n            }\n        }\n        return FALSE;\n    }\n}\n```\n","author_association":"CONTRIBUTOR","reactions":{"url":"https://api.github.com/repos/bcit-ci/CodeIgniter/issues/comments/8127575/reactions","total_count":0,"+1":0,"-1":0,"laugh":0,"hooray":0,"confused":0,"heart":0,"rocket":0,"eyes":0},"performed_via_github_app":null,"event":"commented","actor":{"login":"Dentxinho","id":1199765,"node_id":"MDQ6VXNlcjExOTk3NjU=","avatar_url":"https://avatars.githubusercontent.com/u/1199765?v=4","gravatar_id":"","url":"https://api.github.com/users/Dentxinho","html_url":"https://github.com/Dentxinho","followers_url":"https://api.github.com/users/Dentxinho